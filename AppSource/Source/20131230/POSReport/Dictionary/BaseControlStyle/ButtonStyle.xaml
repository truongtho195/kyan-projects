				<ControlTemplate TargetType="{x:Type Button}">
					<Grid Width="Auto"
						Height="20"
						x:Name="grid" Background="#02FFFFFF" Cursor="Hand">
						<Rectangle 
							RadiusY="3"
							RadiusX="3"
							StrokeThickness="1"
							x:Name="bg" Opacity="1" Visibility="Collapsed" />
						<Rectangle
							RadiusY="3"
							RadiusX="3"
							StrokeThickness="1"
							x:Name="Light" Margin="1,1,1,9" Opacity="0"  Visibility="Collapsed"/>
						<Rectangle 
							RadiusY="3"
							RadiusX="3"
							StrokeThickness="1"
							x:Name="Focused" Opacity="0" Margin="1" StrokeDashArray="2" Visibility="Collapsed"/>
						<ContentPresenter x:Name="contentPresenter" HorizontalAlignment="Left"
							VerticalAlignment="Center"
							SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
							RecognizesAccessKey="True"
							RenderTransformOrigin="0.5,0.5" Margin="5,0,0,0" >
