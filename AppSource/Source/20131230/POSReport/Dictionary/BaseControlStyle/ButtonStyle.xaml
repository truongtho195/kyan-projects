						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="Cursor" TargetName="grid" Value="Hand"/>
						</Trigger>
					</ControlTemplate.Triggers>
						
				</ControlTemplate>
			</Setter.Value>
