                                          Offset="1" />
                            <GradientStop Color="#FF90B0D6"
                                          Offset="0.501" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="BorderBrush"
                        Value="{x:Null}" />
