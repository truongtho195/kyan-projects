<UserControl x:Class="CPC.POS.View.EmployeeInformationView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:addressControl="clr-namespace:CPCToolkitExt.AddressControl;assembly=CPCToolkitExt"
             xmlns:Anno="http://schemas.microPAsoft.com/expression/blend/extensions/annotations/2008"
             xmlns:Anno1="http://schemas.microsoft.com/expression/blend/extensions/annotations/2008"
             xmlns:autoCompleteBox="clr-namespace:CPCToolkitExt.AutoCompleteControl;assembly=CPCToolkitExt"
             xmlns:comboBoxControl="clr-namespace:CPCToolkitExt.ComboBoxControl;assembly=CPCToolkitExt"
             xmlns:command="clr-namespace:CPC.Toolkit.Base"
             xmlns:commandReference="clr-namespace:CPC.Toolkit.Command"
             xmlns:control="clr-namespace:Xceed.Wpf.Toolkit;assembly=WPFToolkit.Extended"
             xmlns:converter="clr-namespace:CPC.Converter"
             xmlns:cpcBehavior="clr-namespace:CPC.Toolkit.Behavior"
             xmlns:cpcControl="clr-namespace:CPCToolkitExt.TextBoxControl;assembly=CPCToolkitExt"
             xmlns:CPCOtherControl="clr-namespace:CPCToolkitExt.OtherControl;assembly=CPCToolkitExt"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dataGridControl="clr-namespace:CPCToolkitExt.DataGridControl;assembly=CPCToolkitExt"
             xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
             xmlns:helper="clr-namespace:CPC.Helper"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             xmlns:local="clr-namespace:CPC.POS"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:Microsoft_Windows_Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Luna"
             xmlns:textBoxControl="clr-namespace:CPCToolkitExt.TextBoxControl;assembly=CPCToolkitExt"
             xmlns:userControl="clr-namespace:CPC.Control"
             xmlns:wpfControl="clr-namespace:Xceed.Wpf.Toolkit;assembly=WPFToolkit.Extended"
             x:Name="UserControl"
             Background="{DynamicResource UserControlBackground}"
             Foreground="White"
             mc:Ignorable="d Anno Anno1">
    <UserControl.Resources>
        <converter:BoolToVisibilityConverter x:Key="boolToVisibilityConverter" />
        <converter:EnumToCollapsedConverter x:Key="enumToCollapsedConverter" />
        <converter:BooleanToIntegerConverter x:Key="booleanToIntegerConverter" />
        <converter:ComboxItemToTextConverter x:Key="comboxItemToTextConverter" />
        <converter:EnumToBoolConverter x:Key="enumToBoolConverter" />
        <converter:StringFormatConverter x:Key="stringFormatConverter" />
        <converter:StringToImageSourceConverter x:Key="stringToImageSourceConverter" />
        <converter:PermissionConverter x:Key="permissionConverter" />

        <Style x:Key="myButtonFingerprintStyle"
               TargetType="{x:Type Button}">
            <Setter Property="Foreground"
                    Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" />
            <Setter Property="HorizontalContentAlignment"
                    Value="Center" />
            <Setter Property="VerticalContentAlignment"
                    Value="Center" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Grid x:Name="grid"
                              Width="Auto"
                              Height="Auto"
                              Background="#02FFFFFF"
                              Cursor="Hand">
                            <Rectangle x:Name="bg"
                                       Fill="{DynamicResource ButtonNormalBackground}"
                                       Opacity="1"
                                       RadiusX="3"
                                       RadiusY="3"
                                       Stroke="{DynamicResource ButtonNormalBorder}"
                                       StrokeThickness="1" />
                            <Rectangle x:Name="Focused"
                                       Margin="2"
                                       Opacity="0"
                                       RadiusX="3"
                                       RadiusY="3"
                                       Stroke="{DynamicResource ButtonNormal_IsKeyboardFocused_Border}"
                                       StrokeDashArray="2"
                                       StrokeThickness="1" />
                            <ContentPresenter x:Name="contentPresenter"
                                              HorizontalAlignment="Stretch"
                                              VerticalAlignment="Stretch"
                                              RecognizesAccessKey="True"
                                              RenderTransformOrigin="0.5,0.5"
                                              SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                                <ContentPresenter.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform />
                                        <SkewTransform />
                                        <RotateTransform />
                                        <TranslateTransform />
                                    </TransformGroup>
                                </ContentPresenter.RenderTransform>
                            </ContentPresenter>
                            <Rectangle x:Name="Light"
                                       Margin="1,1,1,11"
                                       d:IsHidden="True"
                                       Fill="{DynamicResource ButtonNormalLight}"
                                       Opacity="0.9"
                                       RadiusX="3"
                                       RadiusY="3"
                                       StrokeThickness="1"
                                       Visibility="Visible" />
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsKeyboardFocused"
                                     Value="True">
                                <Setter TargetName="bg"
                                        Property="Opacity"
                                        Value="1" />
                                <Setter TargetName="Focused"
                                        Property="Opacity"
                                        Value="1" />
                                <Setter TargetName="Light"
                                        Property="Opacity"
                                        Value="1" />
                                <Setter TargetName="bg"
                                        Property="Fill"
                                        Value="{DynamicResource ButtonNormal_IsKeyboardFocused_Background}" />
                                <Setter TargetName="Light"
                                        Property="Fill"
                                        Value="{DynamicResource ButtonNormalLight}" />
                            </Trigger>
                            <Trigger Property="IsMouseOver"
                                     Value="True">
                                <Setter TargetName="bg"
                                        Property="Opacity"
                                        Value="1" />
                                <Setter TargetName="Light"
                                        Property="Opacity"
                                        Value="1" />
                                <Setter TargetName="bg"
                                        Property="Fill"
                                        Value="{DynamicResource ButtonNormal_IsMouseOver_Background}" />
                            </Trigger>
                            <Trigger Property="IsPressed"
                                     Value="True">
                                <Setter TargetName="bg"
                                        Property="Opacity"
                                        Value="1" />
                                <Setter TargetName="contentPresenter"
                                        Property="RenderTransform">
                                    <Setter.Value>
                                        <TransformGroup>
                                            <ScaleTransform />
                                            <SkewTransform />
                                            <RotateTransform />
                                            <TranslateTransform X="0.5"
                                                                Y="0.5" />
                                        </TransformGroup>
                                    </Setter.Value>
                                </Setter>
                                <Setter TargetName="Light"
                                        Property="Opacity"
                                        Value="1" />
                                <Setter TargetName="bg"
                                        Property="Fill"
                                        Value="{DynamicResource ButtonNormal_IsPress_Background}" />
                            </Trigger>
                            <Trigger Property="IsEnabled"
                                     Value="False">
                                <Setter TargetName="bg"
                                        Property="Opacity"
                                        Value="1" />
                                <Setter TargetName="Light"
                                        Property="Opacity"
                                        Value="1" />
                                <Setter TargetName="bg"
                                        Property="Fill"
                                        Value="{DynamicResource ButtonNormal_IsEnable_Background}" />
                                <Setter TargetName="grid"
                                        Property="Opacity"
                                        Value="0.5" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <!--  ListBoxItemIsDeleteCollapsedStyle  -->
        <Style x:Key="ListBoxItemIsDeleteCollapsedStyle"
               TargetType="{x:Type ListBoxItem}">
            <Setter Property="BorderBrush"
                    Value="{DynamicResource TabControlBorder}" />
            <Setter Property="Foreground"
                    Value="{DynamicResource TabItemControlForeground}" />
            <Setter Property="Background"
                    Value="{DynamicResource TabControlBackground}" />
            <!--
                <Setter Property="Visibility"
                Value="{Binding IsDeleted, Converter={StaticResource reserseBoolToVisibilityConverter}}" />
            -->
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ListBoxItem}">
                        <Border Background="{TemplateBinding Background}">
                            <ContentPresenter ContentSource="Content" />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsSelected"
                         Value="True">
                    <!--
                        <Setter Property="Panel.ZIndex"
                        Value="1" />
                    -->
                    <Setter Property="Visibility"
                            Value="Visible" />
                    <Setter Property="Background"
                            Value="{DynamicResource TabItemControl_IsSelectedBackground}" />
                </Trigger>
                <Trigger Property="IsSelected"
                         Value="False">
                    <Setter Property="Visibility"
                            Value="Collapsed" />
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="myComboBoxItemStyle"
               TargetType="{x:Type ComboBoxItem}">
            <Setter Property="HorizontalContentAlignment"
                    Value="{Binding HorizontalContentAlignment, RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}" />
            <Setter Property="VerticalContentAlignment"
                    Value="{Binding VerticalContentAlignment, RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}" />
            <Setter Property="Padding"
                    Value="3,0,3,0" />
            <Setter Property="Background"
                    Value="Transparent" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ComboBoxItem}">
                        <Border x:Name="Bd"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Padding="{TemplateBinding Padding}"
                                SnapsToDevicePixels="true">
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                              VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                              SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsHighlighted"
                                     Value="true">
                                <Setter TargetName="Bd"
                                        Property="Background"
                                        Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}" />
                                <Setter Property="Foreground"
                                        Value="{DynamicResource {x:Static SystemColors.HighlightTextBrushKey}}" />
                            </Trigger>
                            <Trigger Property="IsEnabled"
                                     Value="false">
                                <Setter Property="Foreground"
                                        Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <DataTrigger Binding="{Binding IsSelected}"
                             Value="False">
                    <Setter Property="IsEnabled"
                            Value="False" />
                    <Setter Property="Visibility"
                            Value="Collapsed" />
                </DataTrigger>
            </Style.Triggers>
        </Style>
        <DataTemplate x:Key="TabInfoHeaderTemplate">
            <TextBlock Text="{DynamicResource EM_Header_EmployeeInfo}" />
        </DataTemplate>
        <DataTemplate x:Key="TabPersonalInfoHeaderTemplate">
            <TextBlock Text="{DynamicResource EM_TextBlock_PersonalInfo}" />
        </DataTemplate>

        <DataTemplate x:Key="TabTimeClockHeaderTemplate">
            <TextBlock Text="{DynamicResource EM_Header_TimeClock}" />
        </DataTemplate>
        <DataTemplate x:Key="TabCredentialHeaderTemplate">
            <TextBlock Text="{DynamicResource EM_Header_Credential}" />
        </DataTemplate>
        <DataTemplate x:Key="TabScheduleHeaderTemplate">
            <TextBlock Text="{DynamicResource EM_Header_Schedule}" />
        </DataTemplate>
        <DataTemplate x:Key="EmployeeHeaderTemplate">
            <TextBlock Text="{DynamicResource EM_Header_EmployeeNo}" />
        </DataTemplate>
        <DataTemplate x:Key="StatusHeaderTemplate">
            <TextBlock Text="{DynamicResource C151}" />
        </DataTemplate>
        <DataTemplate x:Key="TypeHeaderTemplate">
            <TextBlock Text="{DynamicResource EM_Header_EmployeeType}" />
        </DataTemplate>
        <DataTemplate x:Key="FirstNameHeaderTemplate">
            <TextBlock Text="{DynamicResource TimeClockManualEventEditing_DatagridEmployeeName_FirstName}" />
        </DataTemplate>
        <DataTemplate x:Key="LastNameHeaderTemplate">
            <TextBlock Text="{DynamicResource TimeClockManualEventEditing_DatagridEmployeeName_LastName}" />
        </DataTemplate>
        <DataTemplate x:Key="CompanyHeaderTemplate">
            <TextBlock Text="{DynamicResource C170}" />
        </DataTemplate>
        <DataTemplate x:Key="JobTitleHeaderTemplate">
            <TextBlock Text="{DynamicResource EM_Header_JobTitle}" />
        </DataTemplate>
        <DataTemplate x:Key="DepartmentHeaderTemplate">
            <TextBlock Text="{DynamicResource D102}" />
        </DataTemplate>
        <DataTemplate x:Key="PhoneHeaderTemplate">
            <TextBlock Text="{DynamicResource C173}" />
        </DataTemplate>
        <DataTemplate x:Key="EmailHeaderTemplate">
            <TextBlock Text="{DynamicResource C184}" />
        </DataTemplate>
        <DataTemplate x:Key="NewItemHeaderTemplate">
            <TextBlock Text="{DynamicResource C125}" />
        </DataTemplate>
        <DataTemplate x:Key="EditItemHeaderTemplate">
            <TextBlock Text="{DynamicResource C126}" />
        </DataTemplate>
        <DataTemplate x:Key="DeleteItemHeaderTemplate">
            <TextBlock Text="{DynamicResource C127}" />
        </DataTemplate>
        <Style TargetType="{x:Type wpfControl:MaskedTextBox}">
            <Setter Property="VerticalContentAlignment"
                    Value="Center" />
            <Setter Property="BorderThickness"
                    Value="1" />
            <Setter Property="BorderBrush"
                    Value="#FF6B6B6B" />
            <Setter Property="Validation.ErrorTemplate"
                    Value="{x:Null}" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type wpfControl:MaskedTextBox}">
                        <Border x:Name="Bd"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                SnapsToDevicePixels="True">
                            <ScrollViewer x:Name="PART_ContentHost"
                                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsFocused"
                                     Value="True">
                                <Setter Property="Background"
                                        Value="#FFFFF500" />
                            </Trigger>
                            <Trigger Property="Validation.HasError"
                                     Value="True">
                                <Setter TargetName="Bd"
                                        Property="BorderBrush"
                                        Value="#FFFF0000" />
                                <!--  #FFFF0000  -->
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="cpcBehavior:MoveFocusHelper.MoveFocus"
                    Value="True" />
        </Style>
    </UserControl.Resources>
	<UserControl.InputBindings>
		<KeyBinding Command="{Binding HelpCommand}"
                    Gesture="F1" />
	</UserControl.InputBindings>
    <wpfControl:BusyIndicator IsBusy="{Binding IsBusy}">
		<Grid x:Name="grdEmployee">
			<Grid x:Name="grdSearch" d:IsHidden="True">
				<Grid.Visibility>
					<Binding Path="IsSearchMode"
						Mode="OneWay"
						UpdateSourceTrigger="PropertyChanged">
						<Binding.Converter>
							<converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
						</Binding.Converter>
					</Binding>
				</Grid.Visibility>
				<Grid x:Name="grdContentSearch"
					Grid.Row="1"
					Margin="0,0,15,0">
					<Grid.RowDefinitions>
						<RowDefinition Height="85" />
						<RowDefinition />
					</Grid.RowDefinitions>
					<GroupBox x:Name="gbSearch"
						Margin="10,10,0,0">
						<GroupBox.Header>
							<TextBlock x:Name="txtblSearch"
								Foreground="{DynamicResource GroupboxHeaderForeground}"
								Style="{DynamicResource GroupboxTextBlock}"
								Text="Search"
								TextWrapping="NoWrap" />
						</GroupBox.Header>
						<Grid x:Name="grdgbSearch"
							Margin="0,4,0,0">
							<Grid x:Name="grdgbContentSearch">
								<Grid.RowDefinitions>
									<RowDefinition Height="25" />
									<RowDefinition Height="25" />
									<RowDefinition />
								</Grid.RowDefinitions>
								<wpfControl:WatermarkTextBox x:Name="txtKeyword"
									Margin="10,2"
									VerticalContentAlignment="Center"
									Padding="24,0,5,0"
									Text="{Binding FilterText, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
									TextWrapping="NoWrap"
									Watermark="Keyword">
									<i:Interaction.Behaviors>
										<cpcBehavior:AutoFocusBehavior />
									</i:Interaction.Behaviors>
									<wpfControl:WatermarkTextBox.InputBindings>
										<KeyBinding Key="Enter"
											Command="{Binding SearchCommand}"
											CommandParameter="{Binding Text, ElementName=txtKeyword}" />
									</wpfControl:WatermarkTextBox.InputBindings>
								</wpfControl:WatermarkTextBox>
								<Rectangle x:Name="imgFind"
									Width="20"
									Margin="13,3,0,3"
									HorizontalAlignment="Left"
									Fill="{DynamicResource Find}" />
								<Button Style="{DynamicResource ButtonAdvanceSearch}"
									Command="{Binding PopupAdvanceSearchCommand}"
									x:Name="btnAdvanceSearch"
									Grid.Row="1"
									Height="20"
									HorizontalAlignment="Left"
									Width="100"
									VerticalAlignment="Center"
									Content="Advance Search"
									Margin="10,0,0,0" />
							</Grid>
						</Grid>
					</GroupBox>
					<dataGridControl:DataGridControl x:Name="dtgrdEmployeeDirectory"
						Command="{Binding LoadStepCommand}"
						Grid.Row="1"
						Margin="10,10.2,0,10"
						VerticalAlignment="Stretch"
						AutoGenerateColumns="False"
						CanUserAddRows="False"
						CanUserDeleteRows="False"
						cpcBehavior:DoubleClickCommandBehavior.CanDoubleClickOnChild="False"
						cpcBehavior:DoubleClickCommandBehavior.DoubleClickCommand="{Binding DoubleClickViewCommand}"
						cpcBehavior:DoubleClickCommandBehavior.DoubleClickCommandParameter="{Binding SelectedItem, ElementName=dtgrdEmployeeDirectory}"
						CurrentPageIndex="{Binding CurrentPageIndex}"
						DataFilterResult="{Binding FilterEmployeeCollection, Mode=OneWayToSource, UpdateSourceTrigger=PropertyChanged}"
						DisplayItems="{Binding DisplayItems}"
						CurrentColumnCollection="{Binding ColumnCollection, Mode=OneWayToSource, UpdateSourceTrigger=PropertyChanged}"
						IsFilterDataColunm="False"
						IsRollBackData="False"
						ItemsSource="{Binding EmployeeCollection}"
						NumberOfItems="{Binding NumberOfItems}"
						IsColunmHidden="True"
						RowStyle="{StaticResource DataGridToggleRowStyle}"
						SelectionMode="Extended"
						SelectedIndex="0"
						Style="{DynamicResource DatagridWithRowHeader}">
						<dataGridControl:DataGridControl.ContextMenu>
							<ContextMenu Tag="{Binding PlacementTarget, RelativeSource={RelativeSource Mode=Self}}">
								<MenuItem Command="{Binding NewCommand}"
									HeaderTemplate="{StaticResource NewItemHeaderTemplate}"
									IsEnabled="{Binding ., ConverterParameter=EM100-01, Converter={StaticResource permissionConverter}}">
									<MenuItem.Icon>
										<Rectangle Width="16"
											Height="16"
											Fill="{DynamicResource NewItem}"
											Stretch="Fill" />
									</MenuItem.Icon>
								</MenuItem>
								<MenuItem Command="{Binding EditCommand}"
									CommandParameter="{Binding Tag.SelectedItems, RelativeSource={RelativeSource AncestorType={x:Type ContextMenu}, Mode=FindAncestor}}"
									HeaderTemplate="{StaticResource EditItemHeaderTemplate}">
									<MenuItem.Icon>
										<Rectangle Width="16"
											Height="16"
											Fill="{DynamicResource EditItem}"
											Stretch="Fill" />
									</MenuItem.Icon>
								</MenuItem>
								<MenuItem Command="{Binding DeletesCommand}"
									CommandParameter="{Binding Tag.SelectedItems, RelativeSource={RelativeSource AncestorType={x:Type ContextMenu}, Mode=FindAncestor}}"
									HeaderTemplate="{StaticResource DeleteItemHeaderTemplate}"
									IsEnabled="{Binding ., ConverterParameter=EM100-03, Converter={StaticResource permissionConverter}}">
									<MenuItem.Icon>
										<Rectangle Width="16"
											Height="16"
											Fill="{DynamicResource DeleteItem}"
											Stretch="Fill" />
									</MenuItem.Icon>
								</MenuItem>
								<MenuItem Header="{DynamicResource C128}"
									Command="{Binding DuplicateCommand}"
									CommandParameter="{Binding Tag.SelectedItems, RelativeSource={RelativeSource AncestorType={x:Type ContextMenu}, Mode=FindAncestor}}">
									<MenuItem.Icon>
										<Rectangle Width="16"
											Height="16"
											Fill="{DynamicResource DuplicateItem}"
											Stretch="Fill" />
									</MenuItem.Icon>
								</MenuItem>
							</ContextMenu>
						</dataGridControl:DataGridControl.ContextMenu>
						<dataGridControl:DataGridControl.Columns>
							<DataGridTextColumn Binding="{Binding GuestNo}"
								Width="80"
								MinWidth="80"
								dataGridControl:DataGridColumnExtensions.Name="{x:Static local:SearchOptions.AccountNum}"
								HeaderTemplate="{StaticResource EmployeeHeaderTemplate}"
								IsReadOnly="True" />
							<DataGridTextColumn Binding="{Binding IsActived, ConverterParameter=Status, Converter={StaticResource comboxItemToTextConverter}}"
								Width="100"
								MinWidth="80"
								dataGridControl:DataGridColumnExtensions.Name="Status"
								HeaderTemplate="{StaticResource StatusHeaderTemplate}"
								IsReadOnly="True"
								SortMemberPath="Status" />
							<DataGridTemplateColumn Width="120"
								MinWidth="120"
								dataGridControl:DataGridColumnExtensions.Name="{x:Static local:SearchOptions.Type}"
								HeaderTemplate="{StaticResource TypeHeaderTemplate}"
								IsReadOnly="True">
								<DataGridTemplateColumn.CellTemplate>
									<DataTemplate>
										<TextBlock x:Name="txtblBindingCellTemplate"
											Grid.Row="3"
											HorizontalAlignment="Left"
											VerticalAlignment="Center"
											TextWrapping="NoWrap">
											<TextBlock.Style>
												<Style TargetType="{x:Type TextBlock}">
													<Style.Triggers>
														<DataTrigger Binding="{Binding GuestTypeId}"
															Value="1">
															<Setter Property="Text"
																Value="Employee" />
														</DataTrigger>
														<DataTrigger Binding="{Binding GuestTypeId}"
															Value="2">
															<Setter Property="Text"
																Value="In-House" />
														</DataTrigger>
														<DataTrigger Binding="{Binding GuestTypeId}"
															Value="3">
															<Setter Property="Text"
																Value="Salesman" />
														</DataTrigger>
													</Style.Triggers>
												</Style>
											</TextBlock.Style>
										</TextBlock>
									</DataTemplate>
								</DataGridTemplateColumn.CellTemplate>
							</DataGridTemplateColumn>
							<DataGridTextColumn Binding="{Binding FirstName}"
								Width="*"
								MinWidth="150"
								dataGridControl:DataGridColumnExtensions.Name="{x:Static local:SearchOptions.FirstName}"
								HeaderTemplate="{StaticResource FirstNameHeaderTemplate}"
								IsReadOnly="True" />
							<DataGridTextColumn Binding="{Binding LastName}"
								Width="*"
								MinWidth="150"
								dataGridControl:DataGridColumnExtensions.Name="{x:Static local:SearchOptions.LastName}"
								HeaderTemplate="{StaticResource LastNameHeaderTemplate}"
								IsReadOnly="True" />
							<DataGridTextColumn Binding="{Binding Company}"
								Width="150"
								MinWidth="150"
								dataGridControl:DataGridColumnExtensions.Name="{x:Static local:SearchOptions.Company}"
								HeaderTemplate="{StaticResource CompanyHeaderTemplate}"
								IsReadOnly="True" />
							<DataGridTextColumn Binding="{Binding PositionId, ConverterParameter=JobTitle, Converter={StaticResource comboxItemToTextConverter}, Mode=OneWay}"
								Width="150"
								MinWidth="150"
								dataGridControl:DataGridColumnExtensions.Name="{x:Static local:SearchOptions.Position}"
								HeaderTemplate="{StaticResource JobTitleHeaderTemplate}"
								IsReadOnly="True" />
							<DataGridTextColumn Binding="{Binding Department}"
								Width="150"
								MinWidth="150"
								dataGridControl:DataGridColumnExtensions.Name="{x:Static local:SearchOptions.Department}"
								HeaderTemplate="{StaticResource DepartmentHeaderTemplate}"
								IsReadOnly="True" />
							<DataGridTextColumn Binding="{Binding Phone1, ConverterParameter=Phone, Converter={StaticResource stringFormatConverter}}"
								Width="120"
								MinWidth="120"
								dataGridControl:DataGridColumnExtensions.Name="{x:Static local:SearchOptions.Phone}"
								HeaderTemplate="{StaticResource PhoneHeaderTemplate}"
								IsReadOnly="True" />
							<DataGridTextColumn Binding="{Binding Email}"
								Width="*"
								MinWidth="150"
								dataGridControl:DataGridColumnExtensions.Name="{x:Static local:SearchOptions.Email}"
								HeaderTemplate="{StaticResource EmailHeaderTemplate}"
								IsReadOnly="True" />
						</dataGridControl:DataGridControl.Columns>
					</dataGridControl:DataGridControl>
				</Grid>
				<Grid x:Name="grdBtnSearchBackNext"
					Width="15"
					Height="61"
					HorizontalAlignment="Right"
					VerticalAlignment="Center"
					Visibility="Collapsed">
					<Grid.RowDefinitions>
						<RowDefinition />
						<RowDefinition Height="Auto" />
						<RowDefinition Height="Auto" />
					</Grid.RowDefinitions>
					<Button x:Name="tglbtnSearchBack"
						Width="15"
						Margin="0"
						HorizontalAlignment="Right"
						Background="#02FFFFFF"
						BorderBrush="{x:Null}"
						BorderThickness="1"
						ScrollViewer.VerticalScrollBarVisibility="Disabled"
						Style="{DynamicResource ButtonSearch}">
						<Grid x:Name="grdtglbtnSearchBack1">
							<Path x:Name="pathBack1"
								Width="7"
								Height="14"
								HorizontalAlignment="Center"
								VerticalAlignment="Center"
								Data="M9.9166667,11.666667 L6.499667,15.999667 9.9163337,19.666334"
								RenderTransformOrigin="0.5,0.5"
								Stretch="Fill"
								Stroke="White"
								StrokeThickness="2.5" />
						</Grid>
					</Button>
					<Rectangle x:Name="Linebtn"
						Grid.Row="1"
						Height="1"
						VerticalAlignment="Center"
						Fill="White"
						StrokeThickness="0"
						Visibility="Collapsed" />
					<Button x:Name="tglbtnSearchNext"
