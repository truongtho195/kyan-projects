												 cpcBehavior:DoubleClickCommandBehavior.CanDoubleClickOnChild="False"
                                             cpcBehavior:DoubleClickCommandBehavior.DoubleClickCommand="{Binding OpenPaymentCardViewCommand}"
                                                 SelectedItem="{Binding SelectedPaymentDetail, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                 ItemsSource="{Binding PaymentModel.PaymentDetailCollection}"
                                                 
                                                 RowHeight="30"
                                                 
                                                 Style="{DynamicResource DatagridWithoutRowHeader}"
                                                 VisibilityNavigationBar="Collapsed" Margin="0,0.1,0,0" Grid.Row="1">
                        <cpcDataGrid:DataGridControl.Columns>
                        	<DataGridTemplateColumn Width="120"
                        		Header="Methods"
                        		IsReadOnly="true">
                        		<DataGridTemplateColumn.CellTemplate>
                        			<DataTemplate>
                        				<Button x:Name="btnCash"
                        					Grid.Column="1"
                        					Margin="5"
                        					FontSize="12"
                        					Style="{DynamicResource ButtonDatagridPayment}">
                        					<StackPanel x:Name="stkpCash" Orientation="Horizontal">
                        						<Rectangle x:Name="imgCash"
                        							Width="20"
                        							Height="20">
                        							<Rectangle.Fill>
                        								<Binding ConverterParameter="PaymentMethod"
                        									Mode="OneWay"
                        									Path=".">
                        									<!--  Payment methods  -->
                        									<Binding.Converter>
                        										<converter:PaymentButtonIconConverter />
                        									</Binding.Converter>
                        								</Binding>
                        							</Rectangle.Fill>
                        						</Rectangle>
                        						<TextBlock x:Name="txtblCash"
                        							Margin="5,0,0,0"
                        							HorizontalAlignment="Left"
                        							VerticalAlignment="Center"
                        							Style="{DynamicResource ButtonDatagridTextBlockPayment}"
                        							Text="{Binding PaymentMethod}"
                        							TextWrapping="NoWrap" />
                        					</StackPanel>
                        				</Button>
                        			</DataTemplate>
                        		</DataGridTemplateColumn.CellTemplate>
                        	</DataGridTemplateColumn>
                        	<DataGridTemplateColumn Width="*"
                        		MinWidth="100"
                        		Header="Paid">
                        		<DataGridTemplateColumn.CellTemplate>
                        			<DataTemplate>
                        				<TextBlock Text="{Binding Paid,StringFormat={x:Static local:Define.CurrencyFormat}}"
                        					HorizontalAlignment="Right" />
                        			</DataTemplate>
                        		</DataGridTemplateColumn.CellTemplate>
                        		<DataGridTemplateColumn.CellEditingTemplate>
                        			<DataTemplate>
                        				<Grid>
                        					<TextBlock x:Name="tbPaid" 
                                                       Visibility="Collapsed"
                                                       Text="{Binding Paid,StringFormat={x:Static local:Define.CurrencyFormat}}"
                        						HorizontalAlignment="Right" />
                        					<cpcTextBox:TextBoxMoney x:Name="txtPaid"
                        						MinWidth="50"
                        						HorizontalAlignment="Right"
                        						Value="{Binding Paid, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                        				</Grid>
                        				<DataTemplate.Triggers>
                        					<DataTrigger Binding="{Binding EnableRow, Mode=OneWay}" Value="False">
                        						<Setter Property="Visibility"
                        							TargetName="tbPaid"
                        							Value="Visible" />
                        						<Setter Property="Visibility"
                        							TargetName="txtPaid"
                        							Value="Collapsed" />
                        					</DataTrigger>                                            
                        				</DataTemplate.Triggers>
                        			</DataTemplate>
                        		</DataGridTemplateColumn.CellEditingTemplate>
                        	</DataGridTemplateColumn>
                        	<DataGridTemplateColumn Width="200"
                        		Header="Ref #">
                        		<DataGridTemplateColumn.CellTemplate>
                        			<DataTemplate>
                        				<TextBlock Text="{Binding Reference, Mode=OneWay}" />
                        			</DataTemplate>
                        		</DataGridTemplateColumn.CellTemplate>
                        		<DataGridTemplateColumn.CellEditingTemplate>
                        			<DataTemplate>
                        				<Grid>
                        					<TextBlock x:Name="tbRef" Text="{Binding Reference, Mode=OneWay}"
                                                       Visibility="Collapsed" />
                        				<TextBox x:Name="txtRef" Text="{Binding Reference, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
										</Grid>
										<DataTemplate.Triggers>
                        					<DataTrigger Binding="{Binding EnableRow, Mode=OneWay}" Value="False">
                        						<Setter Property="Visibility"
                        							TargetName="tbRef"
                        							Value="Visible" />
                        						<Setter Property="Visibility"
                        							TargetName="txtRef"
                        							Value="Collapsed" />
                        					</DataTrigger>                                            
                        				</DataTemplate.Triggers>
                        			</DataTemplate>
                        		</DataGridTemplateColumn.CellEditingTemplate>
                        	</DataGridTemplateColumn>
