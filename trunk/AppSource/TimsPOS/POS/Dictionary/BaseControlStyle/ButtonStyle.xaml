                            <Setter Property="Opacity"
                                    TargetName="Focused"
                                    Value="1" />
                            <Setter Property="Opacity"
                                    TargetName="Light"
                                    Value="1" />
                            <Setter Property="Fill"
                                    TargetName="bg"
                                    Value="{DynamicResource ButtonKeyboard_IsKeyboardFocused_Background}" />
                            <Setter Property="Fill"
                                    TargetName="Light"
                                    Value="{DynamicResource ButtonKeyboardLight}" />
                        </Trigger>
                        <Trigger Property="IsMouseOver"
                                 Value="True">
                            <Setter Property="Opacity"
                                    TargetName="bg"
                                    Value="1" />
                            <Setter Property="Opacity"
                                    TargetName="Light"
                                    Value="1" />
                            <Setter Property="Fill"
                                    TargetName="bg"
                                    Value="{DynamicResource ButtonKeyboard_IsMouseOver_Background}" />
