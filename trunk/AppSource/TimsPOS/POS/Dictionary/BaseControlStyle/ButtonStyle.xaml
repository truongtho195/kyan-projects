                                 Value="True">
                            <Setter Property="Opacity"
                                    TargetName="bg"
                                    Value="1" />
                            <Setter Property="RenderTransform"
                                    TargetName="contentPresenter">
                                <Setter.Value>
                                    <TransformGroup>
                                        <ScaleTransform />
                                        <SkewTransform />
                                        <RotateTransform />
                                        <TranslateTransform X="0.5"
                                                            Y="0.5" />
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>
                            <Setter Property="Opacity"
                                    TargetName="Light"
                                    Value="1" />
                            <Setter Property="Fill"
                                    TargetName="bg"
                                    Value="{DynamicResource ButtonKeyboard_IsPress_Background}" />
