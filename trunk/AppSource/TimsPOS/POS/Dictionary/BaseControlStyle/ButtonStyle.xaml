    <Style x:Key="PlusMinusToggleButtonStyle"
           TargetType="{x:Type ToggleButton}">
        <Setter Property="FocusVisualStyle"
                Value="{StaticResource ButtonFocusVisual}" />
        <Setter Property="Background"
                Value="White" />
        <Setter Property="Foreground"
                Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" />
        <Setter Property="HorizontalContentAlignment"
                Value="Center" />
        <Setter Property="VerticalContentAlignment"
                Value="Center" />
        <Setter Property="Cursor"
                Value="Hand" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToggleButton}">
                    <Grid>
                        <Rectangle Fill="{DynamicResource DatagridRowCollapse}"
                                   Grid.Row="1"
                                   RenderTransformOrigin="0.5,0.5" Width="Auto" Height="Auto" VerticalAlignment="Stretch">
                            <Rectangle.Visibility>
                                <Binding Path="IsChecked"
                                         RelativeSource="{RelativeSource TemplatedParent}">
                                    <Binding.Converter>
                                        <local:BoolToVisibilityConverter FalseToVisibility="Visible" />
                                    </Binding.Converter>
                                </Binding>
                            </Rectangle.Visibility>
                        </Rectangle>
                        <Rectangle    Grid.Row="1"
                                      RenderTransformOrigin="0.5,0.5" Fill="{DynamicResource DatagridRowExpand}">
                            <Rectangle.Visibility>
                                <Binding Path="IsChecked"
                                         RelativeSource="{RelativeSource TemplatedParent}">
                                    <Binding.Converter>
                                        <local:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                                    </Binding.Converter>
                                </Binding>
                            </Rectangle.Visibility>
                        </Rectangle>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Width" Value="12"/>
        <Setter Property="Height" Value="12"/>
        <Setter Property="MinHeight" Value="0"/>
        <Setter Property="MinWidth" Value="0"/>
    </Style>
