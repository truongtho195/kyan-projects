                        <TextBlock x:Name="txtblTemplateStreet"
                                   Grid.Row="3"
                                   Grid.Column="3"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Street:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateStreet"
                                   Grid.Row="3"
                                   Grid.Column="4"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding AddressModel.AddressLine1}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateCity"
                                   Grid.Row="4"
                                   Grid.Column="3"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="City:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateCity"
                                   Grid.Row="4"
                                   Grid.Column="4"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding AddressModel.City}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateState"
                                   Grid.Row="5"
                                   Grid.Column="3"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="State:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateState"
                                   Grid.Row="5"
                                   Grid.Column="4"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding AddressModel.StateProvinceId,
                                                  ConverterParameter=States,
                                                  Converter={StaticResource comboxItemToTextConverter},
                                                  Mode=OneWay}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateZipCode"
                                   Grid.Row="6"
                                   Grid.Column="3"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Zip Code:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateZipCode"
                                   Grid.Row="6"
                                   Grid.Column="4"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding AddressModel.PostalCode,
                                                  Converter={StaticResource stringFormatConverter},
                                                  ConverterParameter=Zip}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateCountry"
                                   Grid.Row="1"
                                   Grid.Column="5"
                                   Margin="0.786,5.258,0,5.258"
                                   HorizontalAlignment="Left"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Country:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateCountry"
                                   Grid.Row="1"
                                   Grid.Column="6"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding AddressModel.CountryId,
                                                  ConverterParameter=Country,
                                                  Converter={StaticResource comboxItemToTextConverter},
                                                  Mode=OneWay}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplatePhone"
                                   Grid.Row="2"
                                   Grid.Column="5"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Phone:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplatePhone"
                                   Grid.Row="2"
                                   Grid.Column="6"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding Phone1,
                                                  Converter={StaticResource stringFormatConverter},
                                                  ConverterParameter=Phone}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateCellphone"
                                   Grid.Row="3"
                                   Grid.Column="5"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Cellphone:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateCellphone"
                                   Grid.Row="3"
                                   Grid.Column="6"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding CellPhone,
                                                  Converter={StaticResource stringFormatConverter},
                                                  ConverterParameter=Phone}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateEmail"
                                   Grid.Row="4"
                                   Grid.Column="5"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Email:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateEmail"
                                   Grid.Row="4"
                                   Grid.Column="6"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding Email}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateWebsite"
                                   Grid.Row="5"
                                   Grid.Column="5"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Website:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateWebsite"
                                   Grid.Row="5"
                                   Grid.Column="6"
                                   Margin="0"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding Website}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateDOB"
                                   Grid.Row="6"
                                   Grid.Column="5"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="DOB:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateDOB"
                                   Grid.Row="6"
                                   Grid.Column="6"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding PersonalInfoModel.DOB,
                                                  StringFormat=d}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateTaxLocation"
                                   Grid.Row="1"
                                   Grid.Column="7"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Tax Location:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateTaxLocation"
                                   Grid.Row="1"
                                   Grid.Column="8"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="[NOT BINDING]"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateStartDate"
                                   Grid.Row="2"
                                   Grid.Column="7"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Start Date:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateStartDate"
                                   Grid.Row="2"
                                   Grid.Column="8"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding DateCreated,
                                                  StringFormat=\d}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplatePromotionDate"
                                   Grid.Row="3"
                                   Grid.Column="7"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Promotion Date:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplatePromotionDate"
                                   Grid.Row="3"
                                   Grid.Column="8"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="[NOT BINDING]"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateReviewDate"
                                   Grid.Row="4"
                                   Grid.Column="7"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Review Date:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateReviewDate"
                                   Grid.Row="4"
                                   Grid.Column="8"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="[NOT BINDING]"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateDiscount"
                                   Grid.Row="5"
                                   Grid.Column="7"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Discount:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateDiscount"
                                   Grid.Row="5"
                                   Grid.Column="8"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
