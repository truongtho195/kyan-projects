                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Department:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateDepartment"
