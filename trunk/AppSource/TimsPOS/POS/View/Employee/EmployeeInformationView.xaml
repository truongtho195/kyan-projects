<UserControl x:Class="CPC.POS.View.EmployeeInformationView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:addressControl="clr-namespace:CPCToolkitExt.AddressControl;assembly=CPCToolkitExt"
             xmlns:Anno="http://schemas.microPAsoft.com/expression/blend/extensions/annotations/2008"
             xmlns:Anno1="http://schemas.microsoft.com/expression/blend/extensions/annotations/2008"
             xmlns:autoCompleteBox="clr-namespace:CPCToolkitExt.AutoCompleteControl;assembly=CPCToolkitExt"
             xmlns:command="clr-namespace:CPC.Toolkit.Base"
             xmlns:commandReference="clr-namespace:CPC.Toolkit.Command"
             xmlns:control="clr-namespace:Xceed.Wpf.Toolkit;assembly=WPFToolkit.Extended"
             xmlns:converter="clr-namespace:CPC.Converter"
             xmlns:cpcBehavior="clr-namespace:CPC.Toolkit.Behavior"
             xmlns:cpcControl="clr-namespace:CPCToolkitExt.TextBoxControl;assembly=CPCToolkitExt"
             xmlns:CPCOtherControl="clr-namespace:CPCToolkitExt.OtherControl;assembly=CPCToolkitExt"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dataGridControl="clr-namespace:CPCToolkitExt.DataGridControl;assembly=CPCToolkitExt"
             xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
             xmlns:helper="clr-namespace:CPC.Helper"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             xmlns:local="clr-namespace:CPC.POS"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:Microsoft_Windows_Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Luna"
             xmlns:textBoxControl="clr-namespace:CPCToolkitExt.TextBoxControl;assembly=CPCToolkitExt"
             xmlns:userControl="clr-namespace:CPC.Control"
             xmlns:wpfControl="clr-namespace:Xceed.Wpf.Toolkit;assembly=WPFToolkit.Extended"
             x:Name="UserControl"
             Background="{DynamicResource UserControlBackground}"
             Foreground="White"
             mc:Ignorable="d Anno Anno1">
    <UserControl.Resources>
        <converter:BoolToVisibilityConverter x:Key="boolToVisibilityConverter" />
        <converter:EnumToCollapsedConverter x:Key="enumToCollapsedConverter" />
        <converter:BooleanToIntegerConverter x:Key="booleanToIntegerConverter" />
        <converter:ComboxItemToTextConverter x:Key="comboxItemToTextConverter" />
        <converter:EnumToBoolConverter x:Key="enumToBoolConverter" />
        <converter:StringFormatConverter x:Key="stringFormatConverter" />
        <converter:StringToImageSourceConverter x:Key="stringToImageSourceConverter" />
        <DataTemplate x:Key="DataTemplateRowDetailTemplateEmployeeSearch">
            <Grid x:Name="Layout"
                  Width="Auto"
                  Height="202"
                  Margin="0"
                  VerticalAlignment="Top">
                <Rectangle x:Name="BackgroundEmployeeTemplate"
                           Fill="{DynamicResource Datagrid_RowDetailTemplate_Background1}"
                           Stroke="{DynamicResource Datagrid_RowDetailTemplate_Border}" />
                <Border x:Name="brdEmployeeTemplate"
                        Margin="15"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        Background="{DynamicResource Datagrid_RowDetailTemplate_Background2}"
                        BorderBrush="White"
                        BorderThickness="1"
                        d:LayoutOverrides="GridBox">
                    <Grid x:Name="grdTemplateEmployee"
                          Margin="0,0,20,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="120" />
                            <ColumnDefinition Width="100" />
                            <ColumnDefinition Width="170" />
                            <ColumnDefinition Width="90" />
                            <ColumnDefinition Width="219" />
                            <ColumnDefinition Width="70" />
                            <ColumnDefinition Width="200" />
                            <ColumnDefinition Width="105" />
                            <ColumnDefinition Width="105" />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="10" />
                            <RowDefinition Height="25" />
                            <RowDefinition Height="25" />
                            <RowDefinition Height="25" />
                            <RowDefinition Height="25" />
                            <RowDefinition Height="25" />
                            <RowDefinition Height="25" />
                            <RowDefinition Height="10" />
                        </Grid.RowDefinitions>

                        <Grid x:Name="grdPhotoDefault"
                              Grid.Row="2"
                              Grid.RowSpan="4"
                              Width="100"
                              Height="100"
                              Margin="10,0"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Top">
                            <!--  "Visibility" IsNew = True => empty image, show this image  -->
                            <Rectangle x:Name="imgCustomer"
                                       Width="100"
                                       Height="100"
                                       Fill="{DynamicResource Administrator}"
                                       Visibility="{Binding PhotoDefault.IsNew,
                                                            Converter={StaticResource boolToVisibilityConverter},
                                                            ConverterParameter=True}" />
                            <!--  Visibility IsNew = False => has image in database, show this image  -->
                            <Image x:Name="imgDefault"
                                   Width="100"
                                   Height="100"
                                   Source="{Binding PhotoDefault.ImagePath,
                                                    Mode=OneWay,
                                                    UpdateSourceTrigger=PropertyChanged,
                                                    Converter={StaticResource stringToImageSourceConverter}}"
                                   Stretch="Fill"
                                   Visibility="{Binding PhotoDefault.IsNew,
                                                        Converter={StaticResource boolToVisibilityConverter},
                                                        ConverterParameter=False}" />
                        </Grid>
                        <TextBlock x:Name="txtblTemplateEmployeeNo"
                                   Grid.Row="1"
                                   Grid.Column="1"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Employee #:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateEmployeeID"
                                   Grid.Row="1"
                                   Grid.Column="2"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding GuestNo}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateStatus"
                                   Grid.Row="2"
                                   Grid.Column="1"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Status:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateStatus"
                                   Grid.Row="2"
                                   Grid.Column="2"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="{Binding IsActived,
                                                  Converter={StaticResource comboxItemToTextConverter},
                                                  ConverterParameter=Status}"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblTemplateEmployeeType"
                                   Grid.Row="3"
                                   Grid.Column="1"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Employee Type:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateEmployeeType"
                                   Grid.Row="3"
                                   Grid.Column="2"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   TextWrapping="NoWrap">
                            <TextBlock.Style>
                                <Style TargetType="{x:Type TextBlock}">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding GuestTypeId}"
                                                     Value="1">
                                            <Setter Property="Text"
                                                    Value="Employee" />
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding GuestTypeId}"
                                                     Value="2">
                                            <Setter Property="Text"
                                                    Value="In-House" />
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding GuestTypeId}"
                                                     Value="3">
                                            <Setter Property="Text"
                                                    Value="Salesman" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </TextBlock.Style>
                        </TextBlock>
                        <TextBlock x:Name="txtblTemplateFirstName"
                                   Grid.Row="4"
                                   Grid.Column="1"
                                   Margin="0"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="First Name:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateFirstName"
