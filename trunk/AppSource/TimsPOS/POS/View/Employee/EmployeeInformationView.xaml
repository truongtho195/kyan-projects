                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Center"
                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                   Text="Company:"
                                   TextWrapping="NoWrap" />
                        <TextBlock x:Name="txtblBindingTemplateCompany"
