<UserControl x:Class="CPC.POS.View.CustomerView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Anno="http://schemas.microsoft.com/expression/blend/extensions/annotations/2008"
             xmlns:converter="clr-namespace:CPC.Converter"
             xmlns:cpcAddressControl="clr-namespace:CPCToolkitExt.AddressControl;assembly=CPCToolkitExt"
             xmlns:cpcBehavior="clr-namespace:CPC.Toolkit.Behavior"
             xmlns:cpcControl="clr-namespace:CPC.Control"
             xmlns:cpcDataGrid="clr-namespace:CPCToolkitExt.DataGridControl;assembly=CPCToolkitExt"
             xmlns:cpcOtherControl="clr-namespace:CPCToolkitExt.OtherControl;assembly=CPCToolkitExt"
             xmlns:cpcTextbox="clr-namespace:CPCToolkitExt.TextBoxControl;assembly=CPCToolkitExt"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:helper="clr-namespace:CPC.Helper"
             xmlns:local="clr-namespace:CPC.POS"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:Microsoft_Windows_Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Luna"
             xmlns:ms="clr-namespace:System;assembly=mscorlib"
             xmlns:wpfControl="clr-namespace:Xceed.Wpf.Toolkit;assembly=WPFToolkit.Extended"
             x:Name="UserControl"
             Background="{DynamicResource UserControlBackground}"
             mc:Ignorable="d Anno">
    <!--
        xmlns:helper="clr-namespace:CPC.Helper"
        helper:ValidationHelper.IsEnabled="True"
        helper:ValidationHelper.IsValid="{Binding IsValid,
        Mode=OneWayToSource}"
    -->
    <UserControl.Resources>
        <converter:IntegerToBooleanConverter x:Key="integerToBooleanConverter" />
        <converter:DiscountRateToIntegerConverter x:Key="discountRateToIntegerConverter" />
        <converter:IntergerToVisibilityConverter x:Key="intergerToVisibilityConverter" />
        <converter:ComboxItemToTextConverter x:Key="comboxItemToTextConverter" />
        <converter:BoolToVisibilityConverter x:Key="boolToVisibilityConverter" />
        <converter:StringFormatConverter x:Key="stringFormatConverter" />
        <converter:RadioButtonToBoolConverter x:Key="radioButtonToBoolConverter" />
        <converter:BooleanToIntegerConverter x:Key="booleanToIntegerConverter" />
        <converter:EnumToBoolConverter x:Key="enumToBoolConverter" />
        <converter:EnumToCollapsedConverter x:Key="enumToCollapsedConverter" />
        <converter:StringToImageSourceConverter x:Key="stringToImageSourceConverter" />
        <DataTemplate x:Key="DataTemplateRowDetailTemplateCustomerSearch">
            <Grid x:Name="Layout"
                  Height="212"
                  Margin="0"
                  VerticalAlignment="Top">
                <ScrollViewer x:Name="svRowDetailVendorTemplate"
                              Width="Auto"
                              Height="Auto"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Stretch"
                              VerticalScrollBarVisibility="Disabled">
                    <Grid x:Name="grdCustomerTemplate"
                          Height="Auto"
                          Background="{DynamicResource Datagrid_RowDetailTemplate_Background1}">
                        <Border x:Name="brdCustomerTemplate"
                                Margin="15"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                Background="{DynamicResource Datagrid_RowDetailTemplate_Background2}"
                                BorderBrush="White"
                                BorderThickness="1">
                            <Grid x:Name="grdTemplateCustomer"
                                  Margin="0,0,20,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition Width="100" />
                                    <ColumnDefinition Width="170" />
                                    <ColumnDefinition Width="75" />
                                    <ColumnDefinition Width="200" />
                                    <ColumnDefinition Width="105" />
                                    <ColumnDefinition Width="200" />
                                    <ColumnDefinition Width="100" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="10" />
                                    <RowDefinition Height="25" />
                                    <RowDefinition Height="25" />
                                    <RowDefinition Height="25" />
                                    <RowDefinition Height="25" />
                                    <RowDefinition Height="25" />
                                    <RowDefinition Height="25" />
                                    <RowDefinition Height="10" />
                                </Grid.RowDefinitions>
                                <Grid x:Name="grdPhotoDefault"
                                      Grid.Row="2"
                                      Grid.RowSpan="4"
                                      Width="100"
                                      Height="100"
                                      Margin="10,0,0,0"
                                      HorizontalAlignment="Left"
                                      VerticalAlignment="Top">
                                    <!--  "Visibility" IsNew = True => empty image, show this image  -->
                                    <Rectangle x:Name="imgCustomer"
                                               Width="100"
                                               Height="100"
                                               Fill="{DynamicResource Administrator}"
                                               Visibility="{Binding PhotoDefault.IsNew, Converter={StaticResource boolToVisibilityConverter}, ConverterParameter=True}" />
                                    <!--  Visibility IsNew = False => has image in database, show this image  -->
                                    <Image x:Name="imgDefault"
                                           Width="100"
                                           Height="100"
                                           Source="{Binding PhotoDefault.ImagePath, Mode=OneWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource stringToImageSourceConverter}}"
                                           Stretch="Fill"
                                           Visibility="{Binding PhotoDefault.IsNew, Converter={StaticResource boolToVisibilityConverter}, ConverterParameter=False}" />
                                </Grid>
                                <TextBlock x:Name="txtblTemplateAccountNo"
                                           Grid.Row="1"
                                           Grid.Column="1"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Account #:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateCutomerID"
                                           Grid.Row="1"
                                           Grid.Column="2"
                                           Margin="0,0,0,0"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding GuestNo}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateCustomerType"
                                           Grid.Row="2"
                                           Grid.Column="1"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Customer Type:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateCustomerType"
                                           Grid.Row="2"
                                           Grid.Column="2"
                                           Margin="0,0,0,0"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding GuestTypeId, Mode=OneWay, Converter={StaticResource comboxItemToTextConverter}, ConverterParameter=CustomerType}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateRewardMember"
                                           Grid.Row="3"
                                           Grid.Column="1"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Reward Member:"
                                           TextWrapping="NoWrap" />
                                <Rectangle x:Name="imgIssueNewReward"
                                           Grid.Row="3"
                                           Grid.Column="2"
                                           Width="20"
                                           Height="20"
                                           Margin="0,0,0,0"
                                           HorizontalAlignment="Left"
                                           Fill="{DynamicResource IssueReward}"
                                           Visibility="{Binding IsRewardMember, Converter={StaticResource boolToVisibilityConverter}, ConverterParameter=True}" />
                                <TextBlock x:Name="txtblTemplateFirstName"
                                           Grid.Row="4"
                                           Grid.Column="1"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="First Name:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateFirstName"
                                           Grid.Row="4"
                                           Grid.Column="2"
                                           Margin="0,0,0,0"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding FirstName}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateLastName"
                                           Grid.Row="5"
                                           Grid.Column="1"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Last Name:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateLastName"
                                           Grid.Row="5"
                                           Grid.Column="2"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding LastName}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateCompany"
                                           Grid.Row="6"
                                           Grid.Column="1"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Company:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateCompany"
                                           Grid.Row="6"
                                           Grid.Column="2"
                                           Margin="1.898,0,0,0"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding Company}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateJobTitle"
                                           Grid.Row="1"
                                           Grid.Column="3"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Job Title:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateJobTitle"
                                           Grid.Row="1"
                                           Grid.Column="4"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding PositionId, Mode=OneWay, Converter={StaticResource comboxItemToTextConverter}, ConverterParameter=JobTitle}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateStreet"
                                           Grid.Row="2"
                                           Grid.Column="3"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Street:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateStreet"
                                           Grid.Row="2"
                                           Grid.Column="4"
                                           Margin="0,5.861"
                                           HorizontalAlignment="Left"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding AddressModel.AddressLine1}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateCity"
                                           Grid.Row="3"
                                           Grid.Column="3"
                                           Margin="0,5.861"
                                           HorizontalAlignment="Left"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="City:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateCity"
                                           Grid.Row="3"
                                           Grid.Column="4"
                                           Margin="1.898,5.861,0,5.861"
                                           HorizontalAlignment="Left"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding AddressModel.City}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateState"
                                           Grid.Row="4"
                                           Grid.Column="3"
                                           Margin="0,5.861"
                                           HorizontalAlignment="Left"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="State:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateState"
                                           Grid.Row="4"
                                           Grid.Column="4"
                                           Margin="0,5.861"
                                           HorizontalAlignment="Left"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding AddressModel.StateProvinceId, ConverterParameter=States, Converter={StaticResource comboxItemToTextConverter}, Mode=OneWay}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateZipCode"
                                           Grid.Row="5"
                                           Grid.Column="3"
                                           Margin="0,5.861"
                                           HorizontalAlignment="Left"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Zip Code:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateZipCode"
                                           Grid.Row="5"
                                           Grid.Column="4"
                                           Margin="0,5.861"
                                           HorizontalAlignment="Left"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding AddressModel.PostalCode, Converter={StaticResource stringFormatConverter}, ConverterParameter=Zip}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateCountry"
                                           Grid.Row="6"
                                           Grid.Column="3"
                                           Margin="0,5.861"
                                           HorizontalAlignment="Left"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Country:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateCountry"
                                           Grid.Row="6"
                                           Grid.Column="4"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding AddressModel.CountryId, ConverterParameter=Country, Converter={StaticResource comboxItemToTextConverter}, Mode=OneWay}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplatePhone"
                                           Grid.Row="1"
                                           Grid.Column="5"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Phone:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplatePhone"
                                           Grid.Row="1"
                                           Grid.Column="6"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding Phone1, Converter={StaticResource stringFormatConverter}, ConverterParameter=Phone}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateCellphone"
                                           Grid.Row="2"
                                           Grid.Column="5"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Cellphone:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateCellphone"
                                           Grid.Row="2"
                                           Grid.Column="6"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding CellPhone, Converter={StaticResource stringFormatConverter}, ConverterParameter=Phone}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateEmail1"
                                           Grid.Row="3"
                                           Grid.Column="5"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Email:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateEmail"
                                           Grid.Row="3"
                                           Grid.Column="6"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding Email}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateWebsite"
                                           Grid.Row="4"
                                           Grid.Column="5"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Website:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateWebsite"
                                           Grid.Row="4"
                                           Grid.Column="6"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding Website}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateDOB1"
                                           Grid.Row="5"
                                           Grid.Column="5"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="DOB:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateDOB"
                                           Grid.Row="5"
                                           Grid.Column="6"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="{Binding PersonalInfoModel.DOB, StringFormat={x:Static local:Define.DateFormat}}"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateTotalPurchase"
                                           Grid.Row="1"
                                           Grid.Column="7"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Total Purchase:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateTotalPurchase"
                                           Grid.Row="1"
                                           Grid.Column="8"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="$ 1,500.00"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateTotalReward"
                                           Grid.Row="2"
                                           Grid.Column="7"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Total Rewards:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateTotalReward"
                                           Grid.Row="2"
                                           Grid.Column="8"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="$ 150.00"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblTemplateTotalRedeemed"
                                           Grid.Row="3"
                                           Grid.Column="7"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="Total Redeemed:"
                                           TextWrapping="NoWrap" />
                                <TextBlock x:Name="txtblBindingTemplateTotalRedeemed"
                                           Grid.Row="3"
                                           Grid.Column="8"
                                           HorizontalAlignment="Left"
                                           VerticalAlignment="Center"
                                           Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                           Text="$ 80.00"
                                           TextWrapping="NoWrap" />
                            </Grid>
                        </Border>
                    </Grid>
                </ScrollViewer>
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="DataTemplateRowDetailTemplateNote">
            <Grid x:Name="Layout"
                  Height="212"
                  Margin="0"
                  VerticalAlignment="Top">
                <ScrollViewer x:Name="svRowDetailVendorTemplate"
                              Width="Auto"
                              Height="Auto"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Stretch"
                              VerticalScrollBarVisibility="Disabled">
                    <Grid x:Name="grdCustomerTemplate"
                          Height="Auto"
                          Background="{DynamicResource Datagrid_RowDetailTemplate_Background1}">
                        <Border x:Name="brdCustomerTemplate"
                                Margin="15"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                Background="{DynamicResource Datagrid_RowDetailTemplate_Background2}"
                                BorderBrush="{DynamicResource Datagrid_RowDetailTemplate_Border}"
                                BorderThickness="1">
                            <Grid x:Name="grdTemplateCustomerServiceNote"
                                  HorizontalAlignment="Stretch">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="80" />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="10" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="10" />
                                </Grid.RowDefinitions>
                                <Grid x:Name="grdImageName"
                                      Grid.Row="1"
                                      Width="60"
                                      Height="78"
                                      Margin="10,0"
                                      HorizontalAlignment="Left"
                                      VerticalAlignment="Top">
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition Height="25" />
                                    </Grid.RowDefinitions>
                                    <Rectangle x:Name="imgCustomer"
                                               Fill="{DynamicResource Administrator}" />
                                    <TextBlock x:Name="txtblCustomerName"
                                               Grid.Row="2"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               FontWeight="Bold"
                                               Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                               Text="Mike"
                                               TextWrapping="NoWrap" />
                                </Grid>
                                <Border x:Name="brdContentCustomerServiceNote1"
                                        Grid.Row="1"
                                        Grid.Column="2"
                                        Height="Auto"
                                        Margin="0,0,80,0"
                                        Background="{DynamicResource ListBoxItemBackground}"
                                        BorderBrush="{DynamicResource ListBoxItemBorder}"
                                        BorderThickness="1"
                                        CornerRadius="5">
                                    <Grid x:Name="grdContentCustomerServiceNote1"
                                          Visibility="Visible">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="60" />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="25" />
                                            <RowDefinition Height="0" />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <TextBlock x:Name="txtblDate"
                                                   Margin="10,0,0,0"
                                                   HorizontalAlignment="Left"
                                                   VerticalAlignment="Center"
                                                   Text="Date:"
                                                   TextWrapping="NoWrap" />
                                        <TextBlock x:Name="txtblBindingDate"
                                                   Grid.Column="1"
                                                   Margin="0"
                                                   HorizontalAlignment="Left"
                                                   VerticalAlignment="Center"
                                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                                   Text="14/06/2012    12:00:00 PM"
                                                   TextWrapping="NoWrap" />
                                        <TextBlock x:Name="txtblSubject"
                                                   Grid.Row="1"
                                                   Margin="10,0,0,0"
                                                   HorizontalAlignment="Left"
                                                   VerticalAlignment="Center"
                                                   Foreground="White"
                                                   Text="Subject:"
                                                   TextWrapping="NoWrap"
                                                   Visibility="Collapsed" />
                                        <TextBlock x:Name="txtblBindingSubject"
                                                   Grid.Row="1"
                                                   Grid.Column="1"
                                                   Margin="0"
                                                   HorizontalAlignment="Left"
                                                   VerticalAlignment="Center"
                                                   Foreground="White"
                                                   Text="Meeting Friend"
                                                   TextWrapping="NoWrap"
                                                   Visibility="Collapsed" />
                                        <TextBlock x:Name="txtblDetails"
                                                   Grid.Row="2"
                                                   Margin="10,5,0,0"
                                                   HorizontalAlignment="Left"
                                                   VerticalAlignment="Top"
                                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                                   Text="Details:"
                                                   TextWrapping="NoWrap" />
                                        <TextBlock x:Name="txtblBindingDetails"
                                                   Grid.Row="2"
                                                   Grid.Column="1"
                                                   Margin="0,5,5,5"
                                                   HorizontalAlignment="Left"
                                                   VerticalAlignment="Stretch"
                                                   FontSize="11"
                                                   Foreground="{DynamicResource Datagrid_RowDetailTemplate_Foreground}"
                                                   Text="This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample This is a Sample "
                                                   TextTrimming="CharacterEllipsis"
                                                   TextWrapping="Wrap" />
                                    </Grid>
                                </Border>
                                <Canvas Grid.Column="1"
                                        Margin="0,59,0,0"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Top">
                                    <Path Canvas.Left="-17.1"
                                          Canvas.Top="-36.4"
                                          Width="14.5"
                                          Height="18.7"
                                          HorizontalAlignment="Center"
                                          VerticalAlignment="Center"
                                          Data="M0.5,14.667 L6,0.5 11.5,14.667"
                                          Fill="{DynamicResource ListBoxItemArrowBackground}"
                                          RenderTransformOrigin="0.5,0.5"
                                          Stretch="Fill"
                                          Stroke="{DynamicResource ListBoxItemBorder}">
                                        <Path.LayoutTransform>
                                            <TransformGroup>
                                                <ScaleTransform />
                                                <SkewTransform />
                                                <RotateTransform Angle="-90" />
                                                <TranslateTransform />
                                            </TransformGroup>
                                        </Path.LayoutTransform>
                                    </Path>
                                </Canvas>
                            </Grid>
                        </Border>
                    </Grid>
                </ScrollViewer>
            </Grid>
        </DataTemplate>
        <ms:Int32 x:Key="PaymentTermDescriptionMaxLength">50</ms:Int32>
        <Style TargetType="{x:Type cpcDataGrid:DataGridControl}">
            <Setter Property="RowHeight"
                    Value="25" />
            <Setter Property="ColumnHeaderHeight"
                    Value="28" />
            <Setter Property="SelectionMode"
                    Value="Single" />
            <Setter Property="ColumnHeaderStyle"
                    Value="{DynamicResource DataGridColumnHeaderBasicStyle}" />
            <Setter Property="CellStyle"
                    Value="{DynamicResource myDataGridCellStyle}" />
            <Setter Property="RowStyle"
                    Value="{DynamicResource myDataGridControlRowStyle}" />
            <Setter Property="BorderBrush"
                    Value="{DynamicResource DatagridBorderBrush}" />
            <Setter Property="BorderThickness"
                    Value="1" />
            <Setter Property="RowDetailsVisibilityMode"
                    Value="VisibleWhenSelected" />
            <Setter Property="ScrollViewer.CanContentScroll"
                    Value="True" />
            <Setter Property="AutoGenerateColumns"
                    Value="True" />
