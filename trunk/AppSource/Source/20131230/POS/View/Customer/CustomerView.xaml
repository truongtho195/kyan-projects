<UserControl x:Class="CPC.POS.View.CustomerView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Anno="http://schemas.microsoft.com/expression/blend/extensions/annotations/2008"
             xmlns:comboBoxControl="clr-namespace:CPCToolkitExt.ComboBoxControl;assembly=CPCToolkitExt"
             xmlns:converter="clr-namespace:CPC.Converter"
             xmlns:cpcAddressControl="clr-namespace:CPCToolkitExt.AddressControl;assembly=CPCToolkitExt"
             xmlns:cpcBehavior="clr-namespace:CPC.Toolkit.Behavior"
             xmlns:cpcControl="clr-namespace:CPC.Control"
             xmlns:cpcDataGrid="clr-namespace:CPCToolkitExt.DataGridControl;assembly=CPCToolkitExt"
             xmlns:cpcOtherControl="clr-namespace:CPCToolkitExt.OtherControl;assembly=CPCToolkitExt"
             xmlns:cpcTextbox="clr-namespace:CPCToolkitExt.TextBoxControl;assembly=CPCToolkitExt"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:helper="clr-namespace:CPC.Helper"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             xmlns:local="clr-namespace:CPC.POS"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:Microsoft_Windows_Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Luna"
             xmlns:wpfControl="clr-namespace:Xceed.Wpf.Toolkit;assembly=WPFToolkit.Extended"
             x:Name="UserControl"
             Background="{DynamicResource AdjustStock}"
             mc:Ignorable="d Anno">
    <!--
        xmlns:helper="clr-namespace:CPC.Helper"
        helper:ValidationHelper.IsEnabled="True"
        helper:ValidationHelper.IsValid="{Binding IsValid,
        Mode=OneWayToSource}"
    -->
    <UserControl.Resources>
        <cpcBehavior:BindingHelper x:Key="customFieldBindingHelper"
                                   Data="{Binding Path=CustomFieldCollection}" />
        <!--  #region Converter  -->
        <converter:IntegerToBooleanConverter x:Key="integerToBooleanConverter" />
        <converter:DiscountRateToIntegerConverter x:Key="discountRateToIntegerConverter" />
        <converter:IntergerToVisibilityConverter x:Key="intergerToVisibilityConverter" />
        <converter:ComboxItemToTextConverter x:Key="comboxItemToTextConverter" />
        <converter:BoolToVisibilityConverter x:Key="boolToVisibilityConverter" />
        <converter:StringFormatConverter x:Key="stringFormatConverter" />
        <converter:RadioButtonToBoolConverter x:Key="radioButtonToBoolConverter" />
        <converter:BooleanToIntegerConverter x:Key="booleanToIntegerConverter" />
        <converter:ReserseBoolConverter x:Key="reserseBoolConverter" />
        <converter:EnumToBoolConverter x:Key="enumToBoolConverter" />
        <converter:EnumToCollapsedConverter x:Key="enumToCollapsedConverter" />
        <converter:StringToImageSourceConverter x:Key="stringToImageSourceConverter" />
        <converter:PermissionConverter x:Key="permissionConverter" />
        <!--  #EndRegion  -->

        <!--  #Region Style  -->
        <Style x:Key="NumberTextBlock"
               TargetType="{x:Type TextBlock}">
            <Setter Property="HorizontalAlignment"
                    Value="Right" />
        </Style>
        <Style x:Key="CenterTextBlockStyle"
               TargetType="{x:Type TextBlock}">
            <Setter Property="HorizontalAlignment"
                    Value="Center" />
        </Style>

        <Style x:Key="ExpiredDateTextBlock"
               TargetType="{x:Type TextBlock}">
            <Style.Triggers>
                <DataTrigger Binding="{Binding IsExpired, Mode=OneWay}"
                             Value="True">
                    <Setter Property="TextDecorations"
                            Value="Strikethrough" />
                    <Setter Property="Foreground"
                            Value="Red" />
                </DataTrigger>
            </Style.Triggers>
        </Style>
        <ControlTemplate x:Key="DataGridCreditCardControlTemplate"
                         TargetType="{x:Type cpcDataGrid:DataGridControl}">
            <Border Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Padding="{TemplateBinding Padding}">
                <Grid x:Name="GRID_ScrollViewer">
                    <ScrollViewer x:Name="DG_ScrollViewer"
                                  HorizontalContentAlignment="Stretch"
                                  VerticalContentAlignment="Stretch"
                                  Focusable="False"
                                  IsDeferredScrollingEnabled="{TemplateBinding ScrollViewer.IsDeferredScrollingEnabled}"
                                  Style="{DynamicResource myEditScrollViewerInDataGridStyle}"
                                  VirtualizingStackPanel.IsVirtualizing="{TemplateBinding VirtualizingStackPanel.IsVirtualizing}"
                                  VirtualizingStackPanel.VirtualizationMode="{TemplateBinding VirtualizingStackPanel.VirtualizationMode}">
                        <Grid x:Name="GRD_CONTENT">
                            <Grid.RowDefinitions>

                                <RowDefinition />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <ItemsPresenter x:Name="itemsPresenter"
                                            Grid.Row="0"
                                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                            d:LayoutOverrides="Width, Height" />
                            <Button x:Name="btnAddNew"
                                    Grid.Row="1"
                                    Height="25"
                                    Margin="10,0,0,0"
                                    HorizontalAlignment="Left"
                                    Command="{Binding DataContext.CreateNewPaymentCardCommand,
                                                      RelativeSource={RelativeSource AncestorType={x:Type UserControl}}}"
                                    Style="{DynamicResource ButtonDatagrid}">
                                <StackPanel x:Name="stkpBtnAddCreditCard"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Orientation="Horizontal">
                                    <Rectangle x:Name="imgAddCreditCard"
                                               Width="13"
                                               Height="13"
                                               Fill="{DynamicResource AddContact}" />
                                    <TextBlock x:Name="txtblAddCreditCard"
                                               Margin="5,0,0,0"
                                               VerticalAlignment="Center"
                                               Style="{DynamicResource ButtonDatagridTextBlock}"
                                               Text="{DynamicResource Text_NewCreditCard}" />
                                </StackPanel>
                            </Button>
                        </Grid>
                    </ScrollViewer>
                </Grid>
            </Border>
        </ControlTemplate>
        <!--  #EndRegion  -->
    </UserControl.Resources>
	<UserControl.InputBindings>
		<KeyBinding Command="{Binding HelpCommand}"
                    Gesture="F1" />
	</UserControl.InputBindings>
    <wpfControl:BusyIndicator IsBusy="{Binding IsBusy, Mode=OneWay}">
        <Grid x:Name="GridLayout">
            <Rectangle x:Name="UserControl_Background_Image"
                       Fill="{DynamicResource UserControl_Background_Image}" />
            <Grid x:Name="grdCustomer" d:IsHidden="True">
                <!--  Search Gird  -->
                <Grid.Visibility>
                    <Binding Mode="OneWay"
                             Path="IsSearchMode"
                             UpdateSourceTrigger="PropertyChanged">
                        <Binding.Converter>
                            <converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                        </Binding.Converter>
                    </Binding>
                </Grid.Visibility>
                <Grid x:Name="grdContentSearch"
                      Grid.Row="1"
                      Margin="0,0,15,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="85" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <GroupBox x:Name="gbSearch"
                              Margin="10,10,0,0">
                        <GroupBox.Header>
                            <TextBlock x:Name="txtblSearch"
                                       Style="{DynamicResource GroupboxTextBlock}"
                                       Text="{DynamicResource C113}"
                                       TextWrapping="NoWrap" />
                        </GroupBox.Header>
                        <Grid x:Name="grdgbSearch"
                              Margin="0,4,0,0">
                            <Grid x:Name="grdgbContentSearch">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="25" />
                                    <RowDefinition Height="25" />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <wpfControl:WatermarkTextBox x:Name="txtKeyword"
                                                             Margin="10,2,0,2"
                                                             VerticalContentAlignment="Center"
                                                             BorderBrush="{DynamicResource TextBoxCanlendarComboboxBorderBrush}"
                                                             Padding="24,0,5,0"
                                                             Style="{DynamicResource WatermarkTextBoxSearch}"
                                                             Text="{Binding FilterText,
                                                                            Mode=TwoWay,
                                                                            UpdateSourceTrigger=PropertyChanged}"
                                                             TextWrapping="NoWrap"
                                                             Watermark="Keyword">
                                    <i:Interaction.Behaviors>
                                        <cpcBehavior:AutoFocusBehavior />
                                    </i:Interaction.Behaviors>
                                    <wpfControl:WatermarkTextBox.InputBindings>
                                        <KeyBinding Key="Enter"
                                                    Command="{Binding SearchCommand}"
                                                    CommandParameter="{Binding Path=Text,
                                                                               ElementName=txtKeyword}" />
                                    </wpfControl:WatermarkTextBox.InputBindings>
                                </wpfControl:WatermarkTextBox>
                                <Rectangle x:Name="imgFind"
                                           Width="20"
                                           Margin="13,3,0,3"
                                           HorizontalAlignment="Left"
                                           Fill="{DynamicResource Find}" />
                                <Button x:Name="btnAdvanceSearch"
                                        Grid.Row="1"
                                        Width="100"
                                        Height="20"
                                        Margin="10,0,0,0"
                                        Command="{Binding SearchAdvanceCommand}"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        Content="{DynamicResource C104}"
                                        Style="{DynamicResource ButtonAdvanceSearch}" />
                            </Grid>
                        </Grid>
                    </GroupBox>
                    <cpcDataGrid:DataGridControl x:Name="dtgrdCustomerDirectory"
                                                 Grid.Row="1"
                                                 Margin="10,10.2,0,10"
                                                 HorizontalAlignment="Stretch"
                                                 VerticalAlignment="Stretch"
                                                 AutoGenerateColumns="False"
                                                 CanUserAddRows="False"
                                                 CanUserDeleteRows="False"
                                                 Command="{Binding LoadStepCommand}"
                                                 cpcBehavior:DoubleClickCommandBehavior.CanDoubleClickOnChild="False"
                                                 cpcBehavior:DoubleClickCommandBehavior.DoubleClickCommand="{Binding DoubleClickViewCommand}"
                                                 cpcBehavior:DoubleClickCommandBehavior.DoubleClickCommandParameter="{Binding Path=SelectedItem,
                                                                                                                              ElementName=dtgrdCustomerDirectory}"
                                                 DataFilterResult="{Binding FilterEmployeeCollection,
                                                                            Mode=OneWayToSource,
                                                                            UpdateSourceTrigger=PropertyChanged}"
                                                 DisplayItems="{Binding NumberOfDisplayItems}"
                                                 IsColunmHidden="True"
                                                 IsFilterDataColunm="False"
                                                 IsPaging="True"
                                                 IsReadOnly="True"
                                                 IsRollBackData="False"
                                                 IsSynchronizedWithCurrentItem="False"
                                                 ItemsSource="{Binding CustomerCollection}"
                                                 NumberOfColumnsDisplayed="17"
                                                 NumberOfItems="{Binding TotalCustomers}"
                                                 RowStyle="{DynamicResource DataGridToggleRowStyle}"
                                                 ScrollViewer.CanContentScroll="True"
                                                 SelectionMode="Extended"
                                                 Style="{DynamicResource DatagridWithRowHeader}">
                        <cpcDataGrid:DataGridControl.ContextMenu>
                            <ContextMenu Tag="{Binding PlacementTarget, RelativeSource={RelativeSource Mode=Self}}">
                                <MenuItem Command="{Binding NewCommand}"
                                          Header="{DynamicResource C125}"
                                          IsEnabled="{Binding .,
                                                              Converter={StaticResource permissionConverter},
                                                              ConverterParameter=SO100-01-01}">
                                    <MenuItem.Icon>
                                        <Rectangle Width="16"
                                                   Height="16"
                                                   Fill="{DynamicResource NewItem}"
                                                   Stretch="Fill" />
                                    </MenuItem.Icon>
                                </MenuItem>
                                <MenuItem Command="{Binding EditItemCommand}"
                                          CommandParameter="{Binding Tag.SelectedItems,
                                                                     RelativeSource={RelativeSource AncestorType=ContextMenu,
                                                                                                    Mode=FindAncestor}}"
                                          Header="{DynamicResource C126}">
                                    <MenuItem.Icon>
                                        <Rectangle Width="16"
                                                   Height="16"
                                                   Fill="{DynamicResource EditItem}"
                                                   Stretch="Fill" />
                                    </MenuItem.Icon>
                                </MenuItem>
                                <MenuItem Command="{Binding DeletesCommand}"
                                          CommandParameter="{Binding Tag.SelectedItems,
                                                                     RelativeSource={RelativeSource AncestorType=ContextMenu,
                                                                                                    Mode=FindAncestor}}"
                                          Header="{DynamicResource C127}"
                                          IsEnabled="{Binding .,
                                                              Converter={StaticResource permissionConverter},
                                                              ConverterParameter=SO100-01-03}">
                                    <MenuItem.Icon>
                                        <Rectangle Width="16"
                                                   Height="16"
                                                   Fill="{DynamicResource DeleteItem}"
                                                   Stretch="Fill" />
                                    </MenuItem.Icon>
                                </MenuItem>
                                <MenuItem Command="{Binding DuplicateItemCommand}"
                                          CommandParameter="{Binding Tag.SelectedItems,
                                                                     RelativeSource={RelativeSource AncestorType=ContextMenu,
                                                                                                    Mode=FindAncestor}}"
                                          Header="{DynamicResource C128}">
                                    <MenuItem.Icon>
                                        <Rectangle Width="16"
                                                   Height="16"
                                                   Fill="{DynamicResource DuplicateItem}"
                                                   Stretch="Fill" />
                                    </MenuItem.Icon>
                                </MenuItem>
                                <MenuItem Command="{Binding MergeCustomerCommand}"
                                          CommandParameter="{Binding Tag.SelectedItems,
                                                                     RelativeSource={RelativeSource AncestorType=ContextMenu,
                                                                                                    Mode=FindAncestor}}"
                                          Header="{DynamicResource CUS_Text_MergeCustomer}"
                                          IsEnabled="{Binding .,
                                                              Converter={StaticResource permissionConverter},
                                                              ConverterParameter=SO100-01-04}">
                                    <MenuItem.Icon>
                                        <Rectangle Width="16"
                                                   Height="16"
                                                   Fill="{DynamicResource MergeItem}"
                                                   Stretch="Fill" />
                                    </MenuItem.Icon>
                                </MenuItem>
                                <MenuItem Command="{Binding SaleOrderCommand}"
                                          CommandParameter="{Binding Tag.SelectedItems,
                                                                     RelativeSource={RelativeSource AncestorType=ContextMenu,
                                                                                                    Mode=FindAncestor}}"
                                          Header="{DynamicResource CUS_Text_SOCustomer}">
                                    <MenuItem.Icon>
                                        <Rectangle Width="16"
                                                   Height="16"
                                                   Fill="{DynamicResource SalesOrder}"
                                                   Stretch="Fill" />
                                    </MenuItem.Icon>
                                </MenuItem>
                            </ContextMenu>
                        </cpcDataGrid:DataGridControl.ContextMenu>
                        <cpcDataGrid:DataGridControl.Columns>

                            <DataGridTextColumn Width="80"
                                                Binding="{Binding GuestNo}"
                                                IsReadOnly="True">

                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource CUS_Text_AccountNum}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="100"
                                                Binding="{Binding StatusItem.Text}">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource CUS_Text_Status}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTemplateColumn Width="35">
                                <DataGridTemplateColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock />
                                    </DataTemplate>
                                </DataGridTemplateColumn.HeaderTemplate>
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <Rectangle x:Name="imgIssueNewReward"
                                                   Width="20"
                                                   Height="20"
                                                   Fill="{Binding MembershipValidated.MemberShipIcon}"
                                                   Visibility="{Binding IsRewardMember,
                                                                        Converter={StaticResource boolToVisibilityConverter},
                                                                        ConverterParameter=True}" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>

                            <DataGridTextColumn Width="150"
                                                Binding="{Binding FirstName}"
                                                IsReadOnly="True">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource Text_FirstName}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="150"
                                                Binding="{Binding LastName}"
                                                IsReadOnly="True">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource Text_LastName}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="150"
                                                Binding="{Binding Company}"
                                                IsReadOnly="True">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource C170}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="150"
                                                Binding="{Binding GroupName,
                                                                  Mode=OneWay}"
                                                IsReadOnly="True">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource CUS_Text_Group}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="150"
                                                Binding="{Binding AddressModel.City}"
                                                IsReadOnly="True">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource C180}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="150"
                                                Binding="{Binding AddressModel.CountryId,
                                                                  Mode=OneWay,
                                                                  Converter={StaticResource comboxItemToTextConverter},
                                                                  ConverterParameter=Country}"
                                                IsReadOnly="True">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource C172}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="120"
                                                Binding="{Binding Phone1,
                                                                  ConverterParameter=Phone,
                                                                  Converter={StaticResource stringFormatConverter},
                                                                  Mode=OneWay}"
                                                IsReadOnly="True">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource C173}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="120"
                                                Binding="{Binding CellPhone,
                                                                  ConverterParameter=Phone,
                                                                  Converter={StaticResource stringFormatConverter},
                                                                  Mode=OneWay}"
                                                IsReadOnly="True">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource Text_Cellphone}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="200"
                                                Binding="{Binding Email}"
                                                IsReadOnly="True">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource C184}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="200"
                                                Binding="{Binding Website}"
                                                IsReadOnly="True">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource C185}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="100"
                                                Binding="{Binding PurchaseDuringTrackingPeriod,
                                                                  ConverterCulture={x:Static local:Define.ConverterCulture},
                                                                  StringFormat={x:Static local:Define.CurrencyFormat}}"
                                                ElementStyle="{StaticResource NumberTextBlock}">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource CUS_Text_TotalPurchase}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <DataGridTextColumn Width="100"
                                                Binding="{Binding TotalRewardRedeemed,
                                                                  ConverterCulture={x:Static local:Define.ConverterCulture},
                                                                  StringFormat={x:Static local:Define.CurrencyFormat}}"
                                                ElementStyle="{StaticResource NumberTextBlock}">
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{DynamicResource CUS_Text_TotalRewardRedeemed}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>

                            <!--  Custom Field need get dynamic label from db in base_CustomField table  -->
                            <DataGridTextColumn Width="100"
                                                Binding="{Binding Custom1}">
                                <DataGridTextColumn.Visibility>
                                    <Binding Mode="OneWay"
                                             Path="Data[0].IsShow"
                                             Source="{StaticResource customFieldBindingHelper}"
                                             UpdateSourceTrigger="PropertyChanged">
                                        <Binding.Converter>
                                            <converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                                        </Binding.Converter>
                                    </Binding>
                                </DataGridTextColumn.Visibility>

                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Tag="{Binding Path=Data[0].IsShow, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}"
                                                   Text="{Binding Path=Data[0].Label, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}" />

                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="100"
                                                Binding="{Binding Custom2}">
                                <DataGridTextColumn.Visibility>
                                    <Binding Mode="OneWay"
                                             Path="Data[1].IsShow"
                                             Source="{StaticResource customFieldBindingHelper}"
                                             UpdateSourceTrigger="PropertyChanged">
                                        <Binding.Converter>
                                            <converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                                        </Binding.Converter>
                                    </Binding>
                                </DataGridTextColumn.Visibility>
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Tag="{Binding Path=Data[1].IsShow, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}"
                                                   Text="{Binding Path=Data[1].Label, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="100"
                                                Binding="{Binding Custom3}">
                                <DataGridTextColumn.Visibility>
                                    <Binding Mode="OneWay"
                                             Path="Data[2].IsShow"
                                             Source="{StaticResource customFieldBindingHelper}"
                                             UpdateSourceTrigger="PropertyChanged">
                                        <Binding.Converter>
                                            <converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                                        </Binding.Converter>
                                    </Binding>
                                </DataGridTextColumn.Visibility>
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Tag="{Binding Path=Data[2].IsShow, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}"
                                                   Text="{Binding Path=Data[2].Label, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="100"
                                                Binding="{Binding Custom4}">
                                <DataGridTextColumn.Visibility>
                                    <Binding Mode="OneWay"
                                             Path="Data[3].IsShow"
                                             Source="{StaticResource customFieldBindingHelper}"
                                             UpdateSourceTrigger="PropertyChanged">
                                        <Binding.Converter>
                                            <converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                                        </Binding.Converter>
                                    </Binding>
                                </DataGridTextColumn.Visibility>
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Tag="{Binding Path=Data[3].IsShow, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}"
                                                   Text="{Binding Path=Data[3].Label, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="100"
                                                Binding="{Binding Custom5}">
                                <DataGridTextColumn.Visibility>
                                    <Binding Mode="OneWay"
                                             Path="Data[4].IsShow"
                                             Source="{StaticResource customFieldBindingHelper}"
                                             UpdateSourceTrigger="PropertyChanged">
                                        <Binding.Converter>
                                            <converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                                        </Binding.Converter>
                                    </Binding>
                                </DataGridTextColumn.Visibility>
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Tag="{Binding Path=Data[4].IsShow, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}"
                                                   Text="{Binding Path=Data[4].Label, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="100"
                                                Binding="{Binding Custom6}">
                                <DataGridTextColumn.Visibility>
                                    <Binding Mode="OneWay"
                                             Path="Data[5].IsShow"
                                             Source="{StaticResource customFieldBindingHelper}"
                                             UpdateSourceTrigger="PropertyChanged">
                                        <Binding.Converter>
                                            <converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                                        </Binding.Converter>
                                    </Binding>
                                </DataGridTextColumn.Visibility>
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Tag="{Binding Path=Data[5].IsShow, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}"
                                                   Text="{Binding Path=Data[5].Label, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="100"
                                                Binding="{Binding Custom7}">
                                <DataGridTextColumn.Visibility>
                                    <Binding Mode="OneWay"
                                             Path="Data[6].IsShow"
                                             Source="{StaticResource customFieldBindingHelper}"
                                             UpdateSourceTrigger="PropertyChanged">
                                        <Binding.Converter>
                                            <converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                                        </Binding.Converter>
                                    </Binding>
                                </DataGridTextColumn.Visibility>
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Tag="{Binding Path=Data[6].IsShow, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}"
                                                   Text="{Binding Path=Data[6].Label, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="100"
                                                Binding="{Binding Custom8}">
                                <DataGridTextColumn.Visibility>
                                    <Binding Mode="OneWay"
                                             Path="Data[7].IsShow"
                                             Source="{StaticResource customFieldBindingHelper}"
                                             UpdateSourceTrigger="PropertyChanged">
                                        <Binding.Converter>
                                            <converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                                        </Binding.Converter>
                                    </Binding>
                                </DataGridTextColumn.Visibility>
                                <DataGridTextColumn.HeaderTemplate>
                                    <DataTemplate>
                                        <TextBlock Tag="{Binding Path=Data[7].IsShow, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}"
                                                   Text="{Binding Path=Data[7].Label, Source={StaticResource customFieldBindingHelper}, Mode=OneWay}" />
                                    </DataTemplate>
                                </DataGridTextColumn.HeaderTemplate>
