                                                            Orientation="Horizontal">
                                                    <Rectangle x:Name="imgIssueNewReward"
                                                               Width="35"
                                                               Height="35"
                                                               Fill="{DynamicResource IssueReward}" />
                                                    <TextBlock x:Name="txtblIssueNewReward"
                                                               Margin="5,0,0,0"
                                                               VerticalAlignment="Center"
                                                               Style="{DynamicResource ButtonSystemTextBlock}"
                                                               Text="{DynamicResource Text_NewIssueReward}"
                                                               TextWrapping="NoWrap"
                                                               Visibility="Collapsed" />
                                                </StackPanel>
                                            </Button>
                                        </Grid>
                                    </GroupBox>
                                    <GroupBox x:Name="gbPurchaseTrackingPeriod"
                                              Width="280"
                                              Margin="427,10,10,0"
                                              HorizontalAlignment="Left"
                                              VerticalAlignment="Top">
                                        <GroupBox.Header>
                                            <TextBlock x:Name="txtblPurchaseTrackingPeriod"
                                                       Style="{DynamicResource GroupboxTextBlock}"
                                                       Text="{DynamicResource RW_CheckBox_ PurchaseTrackingPeriod}"
                                                       TextWrapping="NoWrap" />
                                        </GroupBox.Header>
                                        <Grid x:Name="grdContenPurchaseTrackingPeriod"
                                              Margin="0,4,0,0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="25" />
                                                <RowDefinition Height="25" />
                                                <RowDefinition Height="25" />
                                                <RowDefinition Height="25" />
                                            </Grid.RowDefinitions>
                                            <TextBlock x:Name="txtblTotalReward"
                                                       Margin="10,0,0,0"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       Style="{DynamicResource TextBlockNormal}"
                                                       Text="{DynamicResource CUS_Text_TotalRewardsAvaliable}"
                                                       TextWrapping="Wrap" />
                                            <TextBlock x:Name="txtblBindingTotalReward"
                                                       Margin="0,0,10,0"
                                                       HorizontalAlignment="Right"
                                                       VerticalAlignment="Center"
                                                       Style="{DynamicResource TextBlockNormal}"
                                                       Text="{Binding SelectedCustomer.TotalRewardsAvaliable,
                                                                      Mode=OneWay}"
                                                       TextWrapping="Wrap" />
                                          
                                            <TextBlock x:Name="txtblTotalTotalAvailableRewardRedeemed"
                                                       Grid.Row="1"
                                                       Margin="10,0,0,0"
                                                       HorizontalAlignment="Left"
                                                       VerticalAlignment="Center"
                                                       Style="{DynamicResource TextBlockNormal}"
                                                       Text="{DynamicResource CUS_Text_TotalRewardRedeemed}"
                                                       TextWrapping="Wrap" />
                                            <TextBlock x:Name="txtblBindingTotalTotalAvailableRewardRedeemed"
                                                       Grid.Row="1"
                                                       Margin="0,0,10,0"
                                                       HorizontalAlignment="Right"
                                                       VerticalAlignment="Center"
                                                       Style="{DynamicResource TextBlockNormal}"
                                                       Text="{Binding SelectedCustomer.TotalOfRewardRedeemed,
                                                                      Mode=OneWay}"
                                                       TextWrapping="Wrap" />
                                        </Grid>
                                    </GroupBox>
                                    <GroupBox x:Name="gbRewardsProgramHistory"
                                              Grid.Row="1"
                                              Margin="10,5">
                                        <GroupBox.Header>
                                            <TextBlock x:Name="txtblRewardsProgramHistory"
                                                       FontWeight="Bold"
                                                       Style="{DynamicResource GroupboxTextBlock}"
                                                       Text="{DynamicResource Text_RewardsProgramHistory}"
                                                       TextWrapping="NoWrap" />
                                        </GroupBox.Header>
                                        <Grid x:Name="grdContentRewardsProgramHistory"
                                              Margin="0,4,0,0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="40" />
                                                <RowDefinition Height="Auto"
                                                               MinHeight="0.2" />
                                                <RowDefinition />
                                                <RowDefinition Height="0" />
                                            </Grid.RowDefinitions>
                                            <StackPanel x:Name="stkpFromTo"
                                                        Margin="10,0,5,0"
                                                        Orientation="Horizontal">
                                                <TextBlock x:Name="txtblRewardProgramHistoryFrom"
                                                           Margin="0,2"
                                                           VerticalAlignment="Center"
                                                           Style="{DynamicResource TextBlockNormal}"
                                                           Text="{DynamicResource Text_From}"
                                                           TextWrapping="NoWrap" />
                                                <DatePicker x:Name="dtpRewardProgramHistoryFrom"
                                                            Width="90"
                                                            Height="21"
                                                            Margin="10,2,0,0"
                                                            VerticalAlignment="Center"
                                                            SelectedDate="{Binding StartDateReward,
                                                                                   Mode=TwoWay,
                                                                                   UpdateSourceTrigger=PropertyChanged}">
                                                    <i:Interaction.Behaviors>
                                                        <cpcBehavior:AutoFocusBehavior />
                                                    </i:Interaction.Behaviors>
                                                </DatePicker>
                                                <TextBlock x:Name="txtblRewardProgramHistoryTo"
                                                           Margin="25,2,0,2"
                                                           VerticalAlignment="Center"
                                                           Style="{DynamicResource TextBlockNormal}"
                                                           Text="{DynamicResource Text_To}"
                                                           TextWrapping="NoWrap" />
                                                <DatePicker x:Name="dtpRewardProgramHistoryTo"
                                                            Width="90"
                                                            Height="21"
                                                            Margin="10,2,0,0"
                                                            VerticalAlignment="Center"
                                                            SelectedDate="{Binding EndDateReward,
                                                                                   Mode=TwoWay,
                                                                                   UpdateSourceTrigger=PropertyChanged}" />

                                            </StackPanel>
                                            <cpcDataGrid:DataGridControl x:Name="dtgrdRewardsEarned"
                                                                         Grid.Row="2"
                                                                         Margin="5,0.1,5,0"
                                                                         AutoGenerateColumns="False"
                                                                         CanUserAddRows="False"
                                                                         CanUserDeleteRows="False"
                                                                         IsReadOnly="True"
                                                                         IsSynchronizedWithCurrentItem="False"
                                                                         ItemsSource="{Binding SelectedCustomer.GuestRewardCollection}"
                                                                         NumberOfItems="{Binding SelectedCustomer.TotalRewardHistory,
                                                                                                 Mode=OneWay}"
                                                                         Style="{DynamicResource DatagridWithRowHeader}">
                                                <cpcDataGrid:DataGridControl.Columns>
                                                    <DataGridTextColumn Width="*"
                                                                        Binding="{Binding EarnedDate,
                                                                                          Mode=OneWay,
                                                                                          StringFormat={x:Static local:Define.DateFormat}}"
                                                                        ElementStyle="{StaticResource ExpiredDateTextBlock}">
                                                        <DataGridTextColumn.Header>
                                                            <TextBlock Text="{DynamicResource Text_EarnedDate}" />
                                                        </DataGridTextColumn.Header>
                                                    </DataGridTextColumn>
                                                    <DataGridTextColumn Width="*"
                                                                        Binding="{Binding StatusItem.Text,
                                                                                          Mode=OneWay}"
                                                                        ElementStyle="{StaticResource ExpiredDateTextBlock}">
                                                        <DataGridTextColumn.Header>
                                                            <TextBlock Text="{DynamicResource C151}" />
                                                        </DataGridTextColumn.Header>
                                                    </DataGridTextColumn>
                                                    <DataGridTextColumn Width="*"
                                                                        Binding="{Binding ActivedDate,
                                                                                          Mode=OneWay,
                                                                                          StringFormat={x:Static local:Define.DateFormat}}"
                                                                        ElementStyle="{StaticResource ExpiredDateTextBlock}">
                                                        <DataGridTextColumn.Header>
                                                            <TextBlock Text="{DynamicResource Text_ActiveDate}" />
                                                        </DataGridTextColumn.Header>
                                                    </DataGridTextColumn>
                                                    <DataGridTemplateColumn Width="*">
                                                        <DataGridTemplateColumn.Header>
                                                            <TextBlock Text="{DynamicResource Text_ExpiredDate}" />
                                                        </DataGridTemplateColumn.Header>
                                                        <DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <TextBlock x:Name="tbExpiredDate"
                                                                           Style="{StaticResource ExpiredDateTextBlock}"
                                                                           Text="{Binding ExpireDate,
                                                                                          Mode=OneWay,
                                                                                          StringFormat={x:Static local:Define.DateFormat}}" />
                                                                <DataTemplate.Triggers>
                                                                    <DataTrigger Binding="{Binding ExpireDate, Mode=OneWay, StringFormat={x:Static local:Define.DateFormat}}"
                                                                                 Value="{x:Null}">
                                                                        <Setter TargetName="tbExpiredDate"
                                                                                Property="Text"
                                                                                Value="{DynamicResource SO_Message_RewardNeverExpired}" />
                                                                    </DataTrigger>
                                                                </DataTemplate.Triggers>
                                                            </DataTemplate>
                                                        </DataGridTemplateColumn.CellTemplate>
                                                    </DataGridTemplateColumn>

                                                    <DataGridTemplateColumn Width="*">
                                                        <DataGridTemplateColumn.Header>
                                                            <TextBlock Text="{DynamicResource CUS_Text_RewardValue}" />
                                                        </DataGridTemplateColumn.Header>
                                                        <DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <!--  Reward Setup Amount Currency  -->
                                                                    <TextBlock x:Name="tbRewardValueEarned"
                                                                               HorizontalAlignment="Right"
                                                                               Text="{Binding RewardValueEarned,
                                                                                              ConverterCulture={x:Static local:Define.ConverterCulture},
                                                                                              Mode=OneWay,
                                                                                              StringFormat={x:Static local:Define.CurrencyFormat}}" />


                                                                </Grid>
                                                            </DataTemplate>
                                                        </DataGridTemplateColumn.CellTemplate>
                                                    </DataGridTemplateColumn>

                                                    <DataGridTemplateColumn Width="*">
                                                        <DataGridTemplateColumn.Header>
                                                            <TextBlock Text="{DynamicResource SO_TextBlock_RewardRedeemed}" />
                                                        </DataGridTemplateColumn.Header>
                                                        <DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <!--  Reward Setup Amount Currency  -->
                                                                    <TextBlock x:Name="tbRewardRedeemed"
                                                                               HorizontalAlignment="Right"
                                                                               Text="{Binding TotalRewardRedeemed,
                                                                                              ConverterCulture={x:Static local:Define.ConverterCulture},
                                                                                              Mode=OneWay,
                                                                                              StringFormat={x:Static local:Define.CurrencyFormat}}" />


                                                                </Grid>
                                                            </DataTemplate>
                                                        </DataGridTemplateColumn.CellTemplate>
                                                    </DataGridTemplateColumn>


                                                </cpcDataGrid:DataGridControl.Columns>
                                            </cpcDataGrid:DataGridControl>
                                        </Grid>
                                    </GroupBox>
                                </Grid>
                            </AdornerDecorator>
                        </cpcOtherControl:CPCTabItemControl>
                        <!--  Sale Order History  -->
                        <cpcOtherControl:CPCTabItemControl x:Name="tabitemSOHistory"
                                                           Header="{DynamicResource CUS_Text_SaleHistory}"
                                                           Tag="SOHistoryTab">
                            <cpcOtherControl:CPCTabItemControl.VisibilityTabItem>
                                <Binding Mode="OneWay"
                                         Path="IsVisibleSOHistory"
                                         UpdateSourceTrigger="PropertyChanged">
                                    <Binding.Converter>
                                        <converter:BoolToVisibilityConverter FalseToVisibility="Collapsed" />
                                    </Binding.Converter>
                                </Binding>
                            </cpcOtherControl:CPCTabItemControl.VisibilityTabItem>
                            <AdornerDecorator>
                                <Grid x:Name="grdContentSOHistory">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition />
                                        <RowDefinition Height="25" />
                                        <RowDefinition Height="5" />
                                    </Grid.RowDefinitions>
                                    <cpcDataGrid:DataGridControl x:Name="dtgrdSOHistory"
                                                                 Grid.Row="1"
                                                                 Margin="5,5,5,0"
                                                                 AutoGenerateColumns="False"
                                                                 CanUserAddRows="False"
                                                                 IsPaging="False"
                                                                 IsReadOnly="True"
                                                                 IsRollBackData="False"
                                                                 ItemsSource="{Binding SelectedCustomer.SaleOrderCollection}"
                                                                 Style="{DynamicResource DatagridWithRowHeader}"
                                                                 VisibilityNavigationBar="Collapsed">
                                        <cpcDataGrid:DataGridControl.Columns>

                                            <DataGridTextColumn Width="80"
                                                                Binding="{Binding SONumber, Mode=OneWay}">

                                                <DataGridTextColumn.Header>
                                                    <TextBlock Text="{DynamicResource Text_OrderNum}" />
                                                </DataGridTextColumn.Header>
                                            </DataGridTextColumn>

                                            <DataGridTextColumn Width="70"
                                                                Binding="{Binding OrderDate, Mode=OneWay, StringFormat={x:Static local:Define.DateFormat}}">

                                                <DataGridTextColumn.Header>
                                                    <TextBlock Text="{DynamicResource C148}" />
                                                </DataGridTextColumn.Header>
                                            </DataGridTextColumn>

                                            <DataGridTextColumn Width="100"
                                                                Binding="{Binding ItemStatus.Text,Mode=OneWay}">
                                                <DataGridTextColumn.Header>
                                                    <TextBlock Text="{DynamicResource C151}" />
                                                </DataGridTextColumn.Header>
                                            </DataGridTextColumn>
                                            <DataGridTemplateColumn Width="*">
                                                <DataGridTemplateColumn.Header>
                                                    <TextBlock Text="{DynamicResource Text_OrderTotal}" />
                                                </DataGridTemplateColumn.Header>
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock HorizontalAlignment="Right"
                                                                   Text="{Binding Total, ConverterCulture={x:Static local:Define.ConverterCulture}, Mode=OneWay, StringFormat={x:Static local:Define.CurrencyFormat}}" />
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <DataGridTemplateColumn Width="*">
                                                <DataGridTemplateColumn.Header>
                                                    <TextBlock Text="{DynamicResource C107}" />
                                                </DataGridTemplateColumn.Header>
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock HorizontalAlignment="Right"
                                                                   Text="{Binding Paid, ConverterCulture={x:Static local:Define.ConverterCulture}, Mode=OneWay, StringFormat={x:Static local:Define.CurrencyFormat}}" />
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <DataGridTemplateColumn Width="*">
                                                <DataGridTemplateColumn.Header>
                                                    <TextBlock Text="{DynamicResource C108}" />
                                                </DataGridTemplateColumn.Header>
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock HorizontalAlignment="Right"
                                                                   Text="{Binding Balance, ConverterCulture={x:Static local:Define.ConverterCulture}, Mode=OneWay, StringFormat={x:Static local:Define.CurrencyFormat}}" />
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                        </cpcDataGrid:DataGridControl.Columns>
                                    </cpcDataGrid:DataGridControl>
                                    <Grid x:Name="grdDatagridBottomSOHistory"
                                          Grid.Row="2"
                                          Height="25"
                                          Margin="5,0"
                                          HorizontalAlignment="Stretch"
                                          VerticalAlignment="Bottom">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="25" />
                                            <ColumnDefinition Width="{Binding ElementName=dtgrdSOHistory, Path=Columns[0].ActualWidth}" />
                                            <ColumnDefinition Width="{Binding ElementName=dtgrdSOHistory, Path=Columns[1].ActualWidth}" />
                                            <ColumnDefinition Width="{Binding ElementName=dtgrdSOHistory, Path=Columns[2].ActualWidth}" />
                                            <ColumnDefinition Width="{Binding ElementName=dtgrdSOHistory, Path=Columns[3].ActualWidth}" />
                                            <ColumnDefinition Width="{Binding ElementName=dtgrdSOHistory, Path=Columns[4].ActualWidth}" />
                                            <ColumnDefinition Width="{Binding ElementName=dtgrdSOHistory, Path=Columns[5].ActualWidth}" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Border x:Name="brdPOHistoryBackground"
                                                Grid.ColumnSpan="8"
                                                Background="{DynamicResource DatagridBottomBackground}"
                                                BorderBrush="{DynamicResource DatagridBottomBorder}"
                                                BorderThickness="1,0,1,1"
                                                d:LayoutOverrides="GridBox" />
                                        <TextBlock x:Name="txtblBindingTotal"
                                                   Grid.Column="4"
                                                   Margin="0,0,4,0"
                                                   HorizontalAlignment="Right"
                                                   VerticalAlignment="Center"
                                                   Style="{DynamicResource TextBlockDatagridBottomTotal}"
                                                   Text="{Binding TotalSaleOrder.Total,
                                                                  ConverterCulture={x:Static local:Define.ConverterCulture},
                                                                  Mode=OneWay,
                                                                  StringFormat={x:Static local:Define.CurrencyFormat}}"
                                                   TextWrapping="NoWrap" />
                                        <TextBlock x:Name="txtblBindingPaid"
                                                   Grid.Column="5"
                                                   Margin="0,0,4,0"
                                                   HorizontalAlignment="Right"
                                                   VerticalAlignment="Center"
                                                   Style="{DynamicResource TextBlockDatagridBottomTotal}"
                                                   Text="{Binding TotalSaleOrder.Paid,
                                                                  ConverterCulture={x:Static local:Define.ConverterCulture},
                                                                  Mode=OneWay,
                                                                  StringFormat={x:Static local:Define.CurrencyFormat}}"
                                                   TextWrapping="NoWrap" />

                                        <TextBlock x:Name="txtblBindingBalance"
                                                   Grid.Column="6"
                                                   Margin="0,0,4,0"
                                                   HorizontalAlignment="Right"
                                                   VerticalAlignment="Center"
                                                   Style="{DynamicResource TextBlockDatagridBottomTotal}"
                                                   Text="{Binding TotalSaleOrder.Balance,
                                                                  ConverterCulture={x:Static local:Define.ConverterCulture},
                                                                  Mode=OneWay,
                                                                  StringFormat={x:Static local:Define.CurrencyFormat}}"
                                                   TextWrapping="NoWrap" />
                                    </Grid>
                                </Grid>
                            </AdornerDecorator>
                        </cpcOtherControl:CPCTabItemControl>
                    </cpcOtherControl:CPCTabControl>
                </Grid>
            </Grid>
        </Grid>
    </wpfControl:BusyIndicator>
</UserControl>